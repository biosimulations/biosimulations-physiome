  <component name="J_CKMM">
    <variable units="flux" public_interface="out" name="J_CKMM"/>
    
    <variable units="micromolar" public_interface="in" name="ATP_cyt"/>
    <variable units="micromolar" public_interface="in" name="ADP_cyt"/>
    <variable units="micromolar" public_interface="in" name="PCr_cyt"/>
    <variable units="micromolar" public_interface="in" name="Cr_cyt"/>
    
    <variable units="dimensionless" name="Den_MMCK"/>
    <variable units="micromolar" name="Kia" initial_value="9.0E2"/>
    <variable units="micromolar" name="Kb" initial_value="1.55E4"/>
    <variable units="micromolar" name="Kib" initial_value="3.49E4"/>
    <variable units="micromolar" name="KIb"/>
    <variable units="micromolar" name="Kc"/>
    <variable units="micromolar" name="Kic" initial_value="2.224E2"/>
    <variable units="micromolar" name="Kd" initial_value="1.67E3"/>
    <variable units="micromolar" name="Kid" initial_value="4.73E3"/>
    <variable units="flux" name="Vmax_MM_f" initial_value="1.144E4"/>
    <variable units="flux" name="Vmax_MM_b" initial_value="4.804E4"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_CKMM_calculation">
        <eq/>
        <ci> J_CKMM </ci>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Vmax_MM_f </ci>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> ATP_cyt </ci>
                  <ci> Cr_cyt </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> Kia </ci>
                  <ci> Kb </ci>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> Vmax_MM_b </ci>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> ADP_cyt </ci>
                  <ci> PCr_cyt </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> Kic </ci>
                  <ci> Kd </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <ci> Den_MMCK </ci>
        </apply>
      </apply>
      
      <apply id="Den_MMCK_calculation">
        <eq/>
        <ci> Den_MMCK </ci>
        <apply>
          <plus/>
          <cn cellml:units="dimensionless"> 1.0 </cn>
          <apply>
            <divide/>
            <ci> Cr_cyt </ci>
            <ci> Kib </ci>
          </apply>
          <apply>
            <divide/>
            <ci> PCr_cyt </ci>
            <ci> Kid </ci>
          </apply>
          <apply>
            <times/>
            <ci> ATP_cyt </ci>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <ci> Kia </ci>
              </apply>
              <apply>
                <divide/>
                <ci> Cr_cyt </ci>
                <apply>
                  <times/>
                  <ci> Kia </ci>
                  <ci> Kb </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> ADP_cyt </ci>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <ci> Kic </ci>
              </apply>
              <apply>
                <divide/>
                <ci> PCr_cyt </ci>
                <apply>
                  <times/>
                  <ci> Kid </ci>
                  <ci> Kc </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Cr_cyt </ci>
                <apply>
                  <times/>
                  <ci> Kic </ci>
                  <ci> KIb </ci>
                </apply>
              </apply>
            </apply>
          </apply> 
        </apply> 
      </apply>  

      <apply id="Kc_calculation">
        <eq/>
        <ci> Kc </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> Kic </ci>
            <ci> Kd </ci>
          </apply>
          <ci> Kid </ci>
        </apply>
	  </apply>

      <apply id="KIb_calculation">
        <eq/>
        <ci> KIb </ci>
        <ci> Kib </ci>
      </apply>		
    </math>
  </component>
  
  <component name="J_CKMi">
    <variable units="flux" public_interface="out" name="J_CKMi"/>
    
    <variable units="micromolar" public_interface="in" name="ATP_ims"/>
    <variable units="micromolar" public_interface="in" name="ADP_ims"/>
    <variable units="micromolar" public_interface="in" name="PCr_ims"/>
    <variable units="micromolar" public_interface="in" name="Cr_ims"/>
    
    <variable units="dimensionless" name="Den_MiCK"/>
    <variable units="micromolar" name="Kia" initial_value="7.5E2"/>
    <variable units="micromolar" name="Kb" initial_value="5.2E3"/>
    <variable units="micromolar" name="Kib" initial_value="2.88E4"/>
    <variable units="micromolar" name="KIb"/>
    <variable units="micromolar" name="Kc"/>
    <variable units="micromolar" name="Kic" initial_value="2.048E2"/>
    <variable units="micromolar" name="Kd" initial_value="5.0E2"/>
    <variable units="micromolar" name="Kid" initial_value="1.6E3"/>
    <variable units="flux" name="Vmax_Mi_f" initial_value="8.82E2"/>
    <variable units="flux" name="Vmax_Mi_b" initial_value="3.704E3"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_CKMi_calculation">
        <eq/>
        <ci> J_CKMi </ci>
        <apply>
          <divide/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> Vmax_Mi_f </ci>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> ATP_ims </ci>
                  <ci> Cr_ims </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> Kia </ci>
                  <ci> Kb </ci>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> Vmax_Mi_b </ci>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <ci> ADP_ims </ci>
                  <ci> PCr_ims </ci>
                </apply>
                <apply>
                  <times/>
                  <ci> Kic </ci>
                  <ci> Kd </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <ci> Den_MiCK </ci>
        </apply>
      </apply>
      
      <apply id="Den_MiCK_calculation">
        <eq/>
        <ci> Den_MiCK </ci>
        <apply>
          <plus/>
          <cn cellml:units="dimensionless"> 1.0 </cn>
          <apply>
            <divide/>
            <ci> Cr_ims </ci>
            <ci> Kib </ci>
          </apply>
          <apply>
            <divide/>
            <ci> PCr_ims </ci>
            <ci> Kid </ci>
          </apply>
          <apply>
            <times/>
            <ci> ATP_ims </ci>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <ci> Kia </ci>
              </apply>
              <apply>
                <divide/>
                <ci> Cr_ims </ci>
                <apply>
                  <times/>
                  <ci> Kia </ci>
                  <ci> Kb </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> ADP_ims </ci>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <ci> Kic </ci>
              </apply>
              <apply>
                <divide/>
                <ci> PCr_ims </ci>
                <apply>
                  <times/>
                  <ci> Kid </ci>
                  <ci> Kc </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Cr_ims </ci>
                <apply>
                  <times/>
                  <ci> Kic </ci>
                  <ci> KIb </ci>
                </apply>
              </apply>
            </apply>
          </apply> 
        </apply> 
      </apply>

      <apply id="Kc_calculation">
        <eq/>
        <ci> Kc </ci>
        <apply>
          <divide/>
          <apply>
            <times/>
            <ci> Kic </ci>
            <ci> Kd </ci>
          </apply>
          <ci> Kid </ci>
        </apply>
	  </apply>

      <apply id="KIb_calculation">
        <eq/>
        <ci> KIb </ci>
        <ci> Kib </ci>
      </apply>	  
    </math>
  </component>
  
  <component name="J_hyd">
      <variable units="flux" public_interface="out" name="J_hyd"/>
      
      <variable units="flux" name="H_ATPmax"/>
      <variable units="flux" name="J_hyd_basis_1" initial_value="4.865e2"/>
      <variable units="flux" name="J_hyd_basis_2" initial_value="6.276e2"/>
      <variable units="dimensionless" name="freq_1" initial_value="135"/>
      <variable units="dimensionless" name="freq_2" initial_value="220"/>
      <variable units="second" name="t_cycle_1"/>
      <variable units="second" name="t_cycle_2"/>
      <variable units="second" name="t_cycle"/>
      <variable units="dimensionless" name="nb_of_cycles_1" initial_value="5"/>
      <variable units="second" name="duration_1"/>
      <variable units="second" public_interface="in" name="time"/>
      <variable units="second" name="ltime"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>t_cycle_1</ci>
            <apply>
               <divide/>
               <cn cellml:units="second">60</cn>
               <ci>freq_1</ci>
            </apply>
         </apply>
         
         <apply>
            <eq/>
            <ci>t_cycle_2</ci>
            <apply>
               <divide/>
               <cn cellml:units="second">60</cn>
               <ci>freq_2</ci>
            </apply>
         </apply>
         
         <apply>
            <eq/>
            <ci>duration_1</ci>
            <apply>
               <times/>
               <ci>nb_of_cycles_1</ci>
               <ci>t_cycle_1</ci>
            </apply>
         </apply>
         
         <apply>
            <eq/>
            <ci>ltime</ci>
            <piecewise>
               <piece>
                  <apply>
                     <minus/>
                     <ci>time</ci>
                     <apply>
                        <times/>
                        <ci>t_cycle_1</ci>
                        <apply>
                           <floor/>
                           <apply>
                              <divide/>
                              <ci>time</ci>
                              <ci>t_cycle_1</ci>
                           </apply>
                        </apply>
                     </apply>
                  </apply>
                  <apply>
                     <leq/>
                     <ci>time</ci>
                     <ci>duration_1</ci>
                  </apply>
               </piece>
               <otherwise>
                  <apply>
                     <minus/>
                     <apply>
                        <minus/>
                        <ci>time</ci>
                        <ci>duration_1</ci>
                     </apply>
                     <apply>
                        <times/>
                        <ci>t_cycle_2</ci>
                        <apply>
                           <floor/>
                           <apply>
                              <divide/>
                              <apply>
                                 <minus/>
                                 <ci>time</ci>
                                 <ci>duration_1</ci>
                              </apply>
                              <ci>t_cycle_2</ci>
                           </apply>
                        </apply>
                     </apply>
                  </apply>
               </otherwise>
            </piecewise>
         </apply>
         <apply>
            <eq/>
            <ci>t_cycle</ci>
            <piecewise>
               <piece>
                  <ci>t_cycle_1</ci>
                  <apply>
                     <leq/>
                     <ci>time</ci>
                     <ci>duration_1</ci>
                  </apply>
               </piece>
               <otherwise>
                  <ci>t_cycle_2</ci>
               </otherwise>
            </piecewise>
         </apply>
        
         <apply>
            <eq/>
            <ci>H_ATPmax</ci>
            <piecewise>
               <piece>
                  <apply>
                     <times/>
                     <cn cellml:units="dimensionless">6</cn>
                     <ci>J_hyd_basis_1</ci>
                  </apply>
                  <apply>
                     <leq/>
                     <ci>time</ci>
                     <ci>duration_1</ci>
                  </apply>
               </piece>
               <otherwise>
                  <apply>
                     <times/>
                     <cn cellml:units="dimensionless">6</cn>
                     <ci>J_hyd_basis_2</ci>
                  </apply>
               </otherwise>
            </piecewise>
         </apply>
         
         <apply>
            <eq/>
            <ci>J_hyd</ci>
            <piecewise>
               <piece>
                  <apply>
                     <times/>
                     <apply>
                        <divide/>
                        <apply>
                           <times/>
                           <ci>H_ATPmax</ci>
                           <ci>ltime</ci>
                        </apply>
                        <ci>t_cycle</ci>
                     </apply>
                     <cn cellml:units="dimensionless">6</cn>
                  </apply>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <ci>ltime</ci>
                        <cn cellml:units="second">0</cn>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>ltime</ci>
                        <apply>
                           <times/>
                           <apply>
                              <divide/>
                              <cn cellml:units="dimensionless">1</cn>
                              <cn cellml:units="dimensionless">6</cn>
                           </apply>
                           <ci>t_cycle</ci>
                        </apply>
                     </apply>
                  </apply>
               </piece>
               <piece>
                  <apply>
                     <times/>
                     <ci>H_ATPmax</ci>
                     <apply>
                        <minus/>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                           <times/>
                           <cn cellml:units="dimensionless">6</cn>
                           <apply>
                              <minus/>
                              <apply>
                                 <divide/>
                                 <ci>ltime</ci>
                                 <ci>t_cycle</ci>
                              </apply>
                              <apply>
                                 <divide/>
                                 <cn cellml:units="dimensionless">1</cn>
                                 <cn cellml:units="dimensionless">6</cn>
                              </apply>
                           </apply>
                        </apply>
                     </apply>
                  </apply>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <ci>ltime</ci>
                        <apply>
                           <times/>
                           <apply>
                              <divide/>
                              <cn cellml:units="dimensionless">1</cn>
                              <cn cellml:units="dimensionless">6</cn>
                           </apply>
                           <ci>t_cycle</ci>
                        </apply>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>ltime</ci>
                        <apply>
                           <times/>
                           <apply>
                              <divide/>
                              <cn cellml:units="dimensionless">1</cn>
                              <cn cellml:units="dimensionless">3</cn>
                           </apply>
                           <ci>t_cycle</ci>
                        </apply>
                     </apply>
                  </apply>
               </piece>
               <piece>
                  <cn cellml:units="flux">0</cn>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <ci>ltime</ci>
                        <apply>
                           <times/>
                           <apply>
                              <divide/>
                              <cn cellml:units="dimensionless">1</cn>
                              <cn cellml:units="dimensionless">3</cn>
                           </apply>
                           <ci>t_cycle</ci>
                        </apply>
                     </apply>
                     <apply>
                        <lt/>
                        <ci>ltime</ci>
                        <ci>t_cycle</ci>
                     </apply>
                  </apply>
               </piece>
            </piecewise>
         </apply>
      </math>
   </component>
  
  <component name="J_syn">
    <variable units="flux" public_interface="out" name="J_syn"/>
    
    <variable units="micromolar" public_interface="in" name="Pi_ims"/>
    <variable units="micromolar" public_interface="in" name="ADP_ims"/>
    
    <variable units="dimensionless" name="Den_syn"/>
    <variable units="micromolar" name="KPi" initial_value="8E2"/>
    <variable units="micromolar" name="KADP" initial_value="25"/>
    <variable units="flux" name="V_max_syn" initial_value="1.504E4"/>
	
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_syn_calculation">
        <eq/>
        <ci> J_syn </ci>
        <apply>
          <times/>
          <ci> V_max_syn </ci>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> ADP_ims </ci>
              <ci> Pi_ims </ci>
            </apply>
            <apply>
              <times/>
              <ci> KPi </ci>
              <ci> KADP </ci>
              <ci> Den_syn </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="Den_syn_calculation">
        <eq/>
        <ci> Den_syn </ci>
        <apply>
          <plus/>
          <cn cellml:units="dimensionless"> 1.0 </cn>
          <apply>
            <divide/>
            <ci> ADP_ims </ci>
            <ci> KADP </ci>
          </apply>
          <apply>
            <divide/>
            <ci> Pi_ims </ci>
            <ci> KPi </ci>
          </apply>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> ADP_ims </ci>
              <ci> Pi_ims </ci>
            </apply>
            <apply>
              <times/>
              <ci> KADP </ci>
              <ci> KPi </ci>
            </apply>
          </apply> 
        </apply> 
      </apply>    
    </math>
  </component>
  
