            <math xmlns="http://www.w3.org/1998/Math/MathML" id="rgpdh">
                  <apply>
                        <eq/>
                        <ci>rgpdh</ci>
                        <apply>
                              <times/>
                              <cn cellml:units="dimensionless">0.2</cn>
                              <apply>
                                    <root/>
                                    <apply>
                                          <abs/>
                                          <apply>
                                                <divide/>
                                                <apply>
                                                      <times/>
                                                      <ci>fbp</ci>
                                                      <cn cellml:units="micromolar">1</cn>
