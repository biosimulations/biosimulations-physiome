  </units>
  
  <units name="millimolar">
    <unit units="mole" prefix="milli"/>
    <unit units="litre" exponent="-1"/>
  </units>
  
  <units name="millimolar_per_millisecond">
    <unit units="millimolar"/>
    <unit units="millisecond" exponent="-1.0"/>
  </units>
  
  <units name="molar">
    <unit units="mole"/>
    <unit units="litre" exponent="-1"/>
  </units>
  
