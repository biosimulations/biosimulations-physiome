        <apply>
          <divide/>
          <cn cellml:units="dimensionless"> 1.0 </cn>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
                  <minus/>
