            </apply>
            <apply>
              <times/>
              <ci> kdmc </ci>
              <ci> MC </ci>
            </apply> 
          </apply>
        </apply>
      </apply>

      <apply>
        <eq/> 
        <ci> vsC </ci> 
        <apply>
          <times/>
          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.8 </cn>
          <ci> vstot </ci>
        </apply>
      </apply>
    </math>
  </component>

