      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> MC </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> vsC </ci> 
            <apply>
              <divide/>
              <apply>
                <power/>
                <ci> BN </ci>
                <ci> n </ci>
              </apply>
              <apply>
                <plus/>
                <apply>
                  <power/>
                  <ci> KAC </ci>
                  <ci> n </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> BN </ci>
