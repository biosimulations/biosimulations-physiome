  <component name="environment">
    <variable cmeta:id="environment_time" name="time" public_interface="out" units="hour"/>
  </component>
  
  <component cmeta:id="MP" name="MP">
      
    <variable cmeta:id="MP_MP" initial_value="0.5" name="MP" public_interface="out" units="nanomolar"/>
    
    <variable name="vsP" units="flux"/>
    <variable initial_value="1.1" name="vmP" units="flux"/>
    <variable initial_value="0.01" name="kdmp" units="first_order_rate_constant"/>
    <variable initial_value="0.7" name="KAP" units="nanomolar"/>
    <variable initial_value="0.3" name="KmP" units="nanomolar"/>
    
    <variable name="vstot" public_interface="in" units="flux"/>
    <variable name="n" public_interface="in" units="dimensionless"/>
    <variable name="BN" public_interface="in" units="nanomolar"/>
    <variable name="time" public_interface="in" units="hour"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> MP </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> vsP </ci> 
            <apply>
              <divide/>
              <apply>
                <power/>
                <ci> BN </ci>
                <ci> n </ci>
              </apply>
              <apply>
                <plus/>
                <apply>
                  <power/>
                  <ci> KAP </ci>
                  <ci> n </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> BN </ci>
                  <ci> n </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> vmP </ci> 
              <apply>
                <divide/>
                <ci> MP </ci>
                <apply>
                  <plus/>
                  <ci> KmP </ci>
                  <ci> MP </ci>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> kdmp </ci>
              <ci> MP </ci>
            </apply> 
          </apply>
        </apply>
      </apply>

      <apply>
        <eq/> 
        <ci> vsP </ci> 
        <ci> vstot </ci>
      </apply>
    </math>
  </component>

  <component cmeta:id="MC" name="MC">
      
