    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> CMT </ci> 
        </apply>
        <apply>
          <times/>
          <ci> fm </ci>
          <apply>
