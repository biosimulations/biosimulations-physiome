    <dc:title>Obeyesekere, Zimmerman, Tecarro, Auchmuty, 1999</dc:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://omex-library.org/ed07f7845877286145bc2c19ebcd33c8.omex">
