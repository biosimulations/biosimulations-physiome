    <dc:title>A single compartment model of pacemaking in dissasociated Substantia nigra neurons with hyperbolic tetrahedral geometry</dc:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://omex-library.org/3fa.omex">
    <prism:keyword>neuron</prism:keyword>
