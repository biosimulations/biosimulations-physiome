    <dc:title>Modelling Hypertrophic IP3 Transients in the Cardiac Myocyte (Cooling, Hunter, Crampin, 2007)</dc:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://omex-library.org/39fee05138f428413db61aa29ff6f0de.omex">
