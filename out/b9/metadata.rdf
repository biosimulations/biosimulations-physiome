    <dc:title>Mathematical modeling of mechanically modulated rhythm disturbances in homogeneous and heterogeneous myocardium with attenuated activity of na+ -k+ pump</dc:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://omex-library.org/b9.omex">
    <prism:keyword>cardiac</prism:keyword>
  </rdf:Description>
  <rdf:Description rdf:about="http://omex-library.org/b9.omex">
    <prism:keyword>electrophysiology</prism:keyword>
