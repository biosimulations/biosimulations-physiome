            <minus/>
            <ci> pHpl </ci>
          </apply>
        </apply>
      </apply>

      <apply>
        <eq/> 
        <ci> Hrbc </ci> 
        <apply>
          <divide/>
          <ci> Hpl </ci>
          <ci> Rrbc </ci>
        </apply>
      </apply>
    </math> 
  </component>


