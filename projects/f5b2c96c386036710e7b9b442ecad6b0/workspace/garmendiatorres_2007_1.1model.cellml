						</apply>
						<ci>R2C2</ci>
						<apply>
							<power/>
							<ci>cAMP</ci>
							<ci>two</ci>
						</apply>
					</apply>
					<apply>
						<times/>
						<ci>r</ci>
						<apply>
							<power/>
							<ci>C</ci>
							<ci>two</ci>
						</apply>
						<ci>R2cAMP2</ci>
						<apply>
							<power/>
							<ci>PKAt</ci>
							<ci>two</ci>
						</apply>
					</apply>
				</apply>
			</apply>
