   <component name="adenylate_cyclase">
      <variable initial_value="CYCLa_init" name="CYCLa" public_interface="out" units="dimensionless"/>
      <variable name="CYCLa_init" public_interface="in" units="dimensionless"/>
      <variable name="RGTP" public_interface="in" units="dimensionless"/>
	  <variable name="RASt" public_interface="in" units="micromolar"/>
	  <variable name="k_a" public_interface="in" units="per_micromolar_minute"/>
	  <variable name="k_i" public_interface="in" units="per_minute"/>
	  <variable name="time" public_interface="in" units="minute"/>
	  <variable name="one" public_interface="in" units="dimensionless"/>
	  <math xmlns="http://www.w3.org/1998/Math/MathML">
		<apply>
			<eq/>
			<apply>
				<diff/>
				<bvar>
				<ci>time</ci>
				</bvar>
				<ci>CYCLa</ci>
			</apply>
			<apply>
				<minus/>
				<apply>
					<times/>
					<ci>k_a</ci>
					<ci>RGTP</ci>
					<ci>RASt</ci>
					<apply>
						<minus/>
						<ci>one</ci>
						<ci>CYCLa</ci>
					</apply>
				</apply>
				<apply>
					<times/>
					<ci>k_i</ci>
					<ci>CYCLa</ci>
				</apply>
			</apply>
		</apply>
	  </math>
   </component>
	<component name="cyclic_AMP">
		<variable initial_value="cAMP_init" name="cAMP" private_interface="out" public_interface="out" units="dimensionless"/>
		<variable name="cAMP_init" public_interface="in" units="dimensionless"/>
		<variable name="time" public_interface="in" units="minute"/>
		<variable name="CYCLa" public_interface="in" units="dimensionless"/>
		<variable name="CYCLt" public_interface="in" units="micromolar"/>
		<variable name="PDEt" public_interface="in" units="micromolar"/>
		<variable name="PDEa" public_interface="in" units="dimensionless"/>
		<variable name="PKAt" public_interface="in" units="micromolar"/>
		<variable name="k_s" public_interface="in" units="per_minute"/>
		<variable name="k_d" public_interface="in" units="per_minute"/>
		<variable name="K_md" public_interface="in" units="micromolar"/>
		<variable name="V_PKAact" public_interface="in" units="per_minute"/>
		<variable name="two" public_interface="in" units="dimensionless"/>
		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<apply>
					<diff/>
					<bvar>
					<ci>time</ci>
					</bvar>
					<ci>cAMP</ci>
				</apply>
				<apply>
					<minus/>
					<apply>
						<minus/>
						<apply>
							<times/>
							<ci>k_s</ci>
							<ci>CYCLa</ci>
							<ci>CYCLt</ci>
						</apply>
						<apply>
							<times/>
							<ci>k_d</ci>
							<ci>PDEt</ci>
							<ci>PDEa</ci>
							<apply>
								<divide/>
								<ci>cAMP</ci>
								<apply>
									<plus/>
									<ci>K_md</ci>
									<ci>cAMP</ci>
								</apply>
							</apply>
						</apply>
					</apply>
					<apply>
						<times/>
						<ci>two</ci>
						<ci>V_PKAact</ci>
						<ci>PKAt</ci>
					</apply>
				</apply>
			</apply>
		</math>
	</component>
