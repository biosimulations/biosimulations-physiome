          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> Ca_m </ci>
        </apply>
        <apply>
          <times/>
          <apply>
            <divide/>
            <ci> fm </ci>
            <cn cellml:units="dimensionless"> 60000.0 </cn>
          </apply>
          <apply>
            <minus/>
            <ci> J_uni </ci>
            <ci> J_NaCa </ci>
          </apply>
        </apply> 
      </apply>
    </math>
  </component>
  
