  <component name="cytosolic_ADP">
    <variable units="millimolar" public_interface="out" name="ATP_i"/>  
    <variable units="millimolar" public_interface="out" name="ADP_i"/>
    <variable units="millimolar" public_interface="out" name="glu"/>
    
    <variable units="mM_min_mg_per_ms_nmol" name="gamma1" initial_value="0.00000153"/>
    <variable units="second" name="tau_hyd" initial_value="50.0"/>
    <variable units="dimensionless" name="n_hyd" initial_value="2.7"/>
    <variable units="nanomole_per_min_mg" name="J_hyd"/>
    <variable units="nanomole_per_min_mg_mM" name="k_hyd" initial_value="41.0"/>
    <variable units="nanomole_per_min_mg" name="delta_J_hyd"/>
    <variable units="nanomole_per_min_mg" name="delta_J_hyd_ss"/>
    <variable units="nanomole_per_min_mg" name="delta_J_hyd_max" initial_value="30.1"/>
    <variable units="nanomole_per_min_mg" name="Jp_gly"/>
    <variable units="nanomole_per_min_mg" name="delta_J_gly_total"/>
    <variable units="millimolar" name="K_glu" initial_value="8.7"/>
   
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_ANT"/>
    <variable units="millisecond" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="ADP_i_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> ADP_i </ci>
        </apply>
        <apply>
          <times/>
          <ci> gamma1 </ci>
          <apply>
            <minus/>
            <ci> J_hyd </ci>
            <apply>
              <plus/>
              <ci> J_ANT </ci>
              <ci> Jp_gly </ci>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="ATP_i_calculation">
        <eq/>
        <ci> ATP_i </ci>
        <apply>
          <minus/>
          <cn cellml:units="millimolar"> 2.0 </cn>
          <ci> ADP_i </ci>
        </apply>
      </apply>
      
      <apply id="J_hyd_calculation">
        <eq/>
        <ci> J_hyd </ci>
        <apply>
          <plus/>
          <apply>
            <times/>
            <ci> k_hyd </ci>
            <ci> ATP_i </ci>
          </apply>
          <ci> delta_J_hyd </ci>
        </apply>
      </apply>
      
      <apply id="Jp_gly_calculation">
        <eq/>
        <ci> Jp_gly </ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless"> 2.0 </cn>
          <ci> delta_J_gly_total </ci>
        </apply>
      </apply>
      
      <apply id="delta_J_hyd_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> delta_J_hyd </ci>
        </apply>
        <apply>
          <times/>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <ci> tau_hyd </ci>
          </apply>
          <apply>
            <minus/>
            <ci> delta_J_hyd_ss </ci>
            <ci> delta_J_hyd </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="delta_J_hyd_ss_calculation">
        <eq/>
        <ci> delta_J_hyd_ss </ci>
        <apply>
          <divide/>
          <ci> delta_J_hyd_max </ci>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <power/>
              <apply>
                <divide/>
                <ci> K_glu </ci>
                <ci> glu </ci>
              </apply>
              <ci> n_hyd </ci>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
  <component name="cytosolic_Ca">
    <variable units="micromolar" public_interface="out" name="Ca_i"/>
    
    <variable units="uM_min_mg_per_ms_nmol" name="gamma2" initial_value="0.00153"/>
    <variable units="micromolar_per_ms_fA" name="alpha" initial_value="0.00000678"/>
    <variable units="per_millisecond" name="k_Ca" initial_value="0.19"/>
    <variable units="dimensionless" name="f_i" initial_value="0.01"/>
    
    <variable units="femtoA" public_interface="in" name="i_NS"/>
    <variable units="femtoA" public_interface="in" name="i_Ca_f"/>
    <variable units="femtoA" public_interface="in" name="i_Ca_s"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_uni"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_NaCa"/>
    <variable units="millisecond" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="Ca_i_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> Ca_i </ci>
        </apply>
        <apply>
          <times/>
          <ci> f_i </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <apply>
                <minus/>
                <ci> alpha </ci>
              </apply>
              <apply>
                <plus/>
                <ci> i_NS </ci>
                <ci> i_Ca_f </ci>
                <ci> i_Ca_s </ci>
              </apply>
            </apply>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> gamma2 </ci>
                <apply>
                  <minus/>
                  <ci> J_uni </ci>
                  <ci> J_NaCa </ci>
                </apply> 
              </apply>
              <apply>
                <times/>
                <ci> k_Ca </ci>
                <ci> Ca_i </ci>
              </apply> 
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
  
  <component name="mitochondrial_membrane">
    <variable units="millivolt" public_interface="out" name="delta_psi" initial_value="50.0"/>
    <variable units="nanomole_per_min_mg" public_interface="out" name="proton_motive_force"/>  
    <variable units="nanomole_per_min_mV_mg" public_interface="out" name="g_H" initial_value="0.2"/>
    <variable units="dimensionless" public_interface="out" name="delta_pH" initial_value="-0.4"/>
    
    <variable units="nanomole_per_mV_mg" name="Cmito" initial_value="0.00145"/>
    <variable units="dimensionless" name="pH_i"/>
    <variable units="dimensionless" name="pH_m"/>
      
    <variable units="nanomole_per_min_mg" public_interface="in" name="JH_leak"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="JH_res"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="JH_F1"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_ANT"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_uni"/>
    <variable units="nanomole_per_min_mg" public_interface="in" name="J_NaCa"/>
    <variable units="millijoule_per_mole_kelvin" public_interface="in" name="R"/>
    <variable units="coulomb_per_mole" public_interface="in" name="F"/>
    <variable units="kelvin" public_interface="in" name="T"/>
    <variable units="millivolt" public_interface="in" name="V"/>
    <variable units="millisecond" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="mitochondrial_membrane_voltage_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> delta_psi </ci>
        </apply>
        <apply>
          <times/>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless"> -1.0 </cn>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> 60000.0 </cn>
              <ci> Cmito </ci>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <minus/>
              <ci> JH_res </ci>
            </apply>
            <ci> JH_F1 </ci>
            <ci> J_ANT </ci>
            <ci> JH_leak </ci>
            <apply>
              <times/>
              <ci> J_uni </ci>
              <cn cellml:units="dimensionless"> 2.0 </cn>
            </apply>
            <ci> J_NaCa </ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="delta_pH_calculation">
        <eq/>
        <ci> delta_pH </ci>
        <apply>
          <minus/>
          <ci> pH_i </ci>
          <ci> pH_m </ci>
        </apply>
      </apply>
      
      <apply id="proton_motive_force_calculation">
        <eq/>
        <ci> proton_motive_force </ci>
        <apply>
          <minus/>
          <ci> V </ci>
          <apply>
            <times/>
            <apply>
              <divide/>
              <apply>
                <times/>
                <cn cellml:units="dimensionless"> 2.303 </cn>
                <ci> R </ci>
                <ci> T </ci>
              </apply>
              <ci> F </ci>
            </apply>
            <ci> delta_pH </ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
