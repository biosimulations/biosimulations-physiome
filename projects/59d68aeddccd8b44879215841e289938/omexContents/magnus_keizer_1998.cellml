          <times/>
          <cn cellml:units="dimensionless"> 0.73 </cn>
          <ci> g_Ca </ci>
          <ci> O_s </ci>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> Ca_o </ci>
              <ci> V </ci>
            </apply>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <cn cellml:units="dimensionless"> 2.0 </cn>
                    <ci> F </ci>
                    <ci> V </ci>
                  </apply>
                  <apply>
                    <times/>
                    <ci> R </ci>
                    <ci> T </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    
      <apply id="O_s_calculation">
        <eq/>
        <ci> O_s </ci>
        <apply>
          <divide/>
          <ci> J </ci>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <apply>
