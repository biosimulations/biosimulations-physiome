    <units name="micromolar">
        <unit prefix="micro" units="mole"/>
        <unit exponent="-1" units="litre"/>
    </units>
    <units name="flux">
        <unit units="micromolar"/>
        <unit exponent="-1" units="second"/>
    </units>
    <units name="first_order_rate_constant">
        <unit exponent="-1" units="second"/>
    </units>
    <units name="second_order_rate_constant">
        <unit exponent="-1" units="micromolar"/>
        <unit exponent="-1" units="second"/>
    </units>
    <component name="environment">
        <variable name="time" public_interface="out" units="second"/>
    </component>
    <component cmeta:id="id_000000001" name="total_calcium">
        <variable cmeta:id="total_calcium_Ca_Pr" name="Ca_Pr" public_interface="out" units="micromolar"/>
        <variable cmeta:id="id_000000002" initial_value="90.0" name="Ca_tot" units="micromolar"/>
        <variable name="rho_ER" public_interface="in" units="dimensionless"/>
        <variable name="beta_ER" public_interface="in" units="dimensionless"/>
        <variable name="rho_m" public_interface="in" units="dimensionless"/>
        <variable name="beta_m" public_interface="in" units="dimensionless"/>
        <variable cmeta:id="id_000000003" name="Ca_cyt" public_interface="in" units="micromolar"/>
        <variable cmeta:id="id_000000004" name="Ca_ER" public_interface="in" units="micromolar"/>
        <variable cmeta:id="id_000000005" name="Ca_m" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="Ca_Pr_calculation">
                <eq/>
                <ci> Ca_Pr </ci>
                <apply>
                    <minus/>
                    <ci> Ca_tot </ci>
                    <apply>
                        <plus/>
                        <ci> Ca_cyt </ci>
                        <apply>
                            <times/>
                            <apply>
                                <divide/>
                                <ci> rho_ER </ci>
                                <ci> beta_ER </ci>
                            </apply>
                            <ci> Ca_ER </ci>
                        </apply>
                        <apply>
                            <times/>
                            <apply>
                                <divide/>
                                <ci> rho_m </ci>
                                <ci> beta_m </ci>
                            </apply>
                            <ci> Ca_m </ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000006" name="total_protein">
        <variable cmeta:id="id_000000007" name="Pr" public_interface="out" units="micromolar"/>
        <variable cmeta:id="id_000000008" initial_value="120.0" name="Pr_tot" units="micromolar"/>
        <variable cmeta:id="id_000000009" name="Ca_Pr" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="Pr_calculation">
                <eq/>
                <ci> Pr </ci>
                <apply>
                    <minus/>
                    <ci> Pr_tot </ci>
                    <ci> Ca_Pr </ci>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000010" name="cytosolic_calcium">
        <variable initial_value="0.05" name="Ca_cyt" public_interface="out" units="micromolar"/>
        <variable initial_value="0.1" name="k_plus" units="second_order_rate_constant"/>
        <variable initial_value="0.01" name="k_minus" units="first_order_rate_constant"/>
        <variable name="Pr" public_interface="in" units="micromolar"/>
        <variable name="Ca_Pr" public_interface="in" units="micromolar"/>
        <variable name="J_ch" public_interface="in" units="flux"/>
        <variable cmeta:id="id_000000015" name="J_leak" public_interface="in" units="flux"/>
        <variable cmeta:id="id_000000019" name="J_pump" public_interface="in" units="flux"/>
        <variable name="J_out" public_interface="in" units="flux"/>
        <variable name="J_in" public_interface="in" units="flux"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="Ca_cyt_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> Ca_cyt </ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <plus/>
                        <ci> J_ch </ci>
                        <ci> J_leak </ci>
                        <ci> J_out </ci>
                        <apply>
                            <times/>
                            <ci> k_minus </ci>
                            <ci> Ca_Pr </ci>
                        </apply>
                    </apply>
                    <apply>
                        <plus/>
                        <ci> J_pump </ci>
                        <ci> J_in </ci>
                        <apply>
                            <times/>
                            <ci> k_plus </ci>
                            <ci> Ca_cyt </ci>
                            <ci> Pr </ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000011" name="ER_calcium">
        <variable cmeta:id="id_000000012" initial_value="1.0" name="Ca_ER" public_interface="out" units="micromolar"/>
        <variable cmeta:id="id_000000013" initial_value="0.01" name="rho_ER" public_interface="out" units="dimensionless"/>
        <variable cmeta:id="id_000000014" initial_value="0.0025" name="beta_ER" public_interface="out" units="dimensionless"/>
        <variable name="J_ch" public_interface="in" units="flux"/>
        <variable cmeta:id="id_000000016" name="J_leak" public_interface="in" units="flux"/>
        <variable cmeta:id="id_000000020" name="J_pump" public_interface="in" units="flux"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="Ca_ER_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> Ca_ER </ci>
                </apply>
                <apply>
                    <times/>
                    <apply>
                        <divide/>
                        <ci> beta_ER </ci>
                        <ci> rho_ER </ci>
                    </apply>
                    <apply>
                        <minus/>
                        <ci> J_pump </ci>
                        <apply>
                            <plus/>
                            <ci> J_ch </ci>
                            <ci> J_leak </ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component name="mitochondrial_calcium">
        <variable initial_value="0.4" name="Ca_m" public_interface="out" units="micromolar"/>
        <variable initial_value="0.01" name="rho_m" public_interface="out" units="dimensionless"/>
        <variable initial_value="0.0025" name="beta_m" public_interface="out" units="dimensionless"/>
        <variable name="J_in" public_interface="in" units="flux"/>
        <variable name="J_out" public_interface="in" units="flux"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="Ca_m_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> Ca_m </ci>
                </apply>
                <apply>
                    <times/>
                    <apply>
                        <divide/>
                        <ci> beta_m </ci>
                        <ci> rho_m </ci>
                    </apply>
                    <apply>
                        <minus/>
                        <ci> J_in </ci>
                        <ci> J_out </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000022" name="ATP_dependent_Ca_uptake_into_the_ER">
        <variable cmeta:id="ATP_dependent_Ca_uptake_into_the_ER_J_pump" name="J_pump" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000021" initial_value="20.0" name="k_pump" units="first_order_rate_constant"/>
        <variable name="Ca_cyt" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_pump_calculation">
                <eq/>
                <ci> J_pump </ci>
                <apply>
                    <times/>
                    <ci> k_pump </ci>
                    <ci> Ca_cyt </ci>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000023" name="Ca_efflux_from_the_ER">
        <variable cmeta:id="Ca_efflux_from_the_ER_J_ch" name="J_ch" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000024" initial_value="4100.0" name="k_ch" units="first_order_rate_constant"/>
        <variable initial_value="5.0" name="K1" units="micromolar"/>
        <variable name="Ca_cyt" public_interface="in" units="micromolar"/>
        <variable name="Ca_ER" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_ch_calculation">
                <eq/>
                <ci> J_ch </ci>
                <apply>
                    <times/>
                    <ci> k_ch </ci>
                    <apply>
                        <divide/>
                        <apply>
                            <power/>
                            <ci> Ca_cyt </ci>
                            <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <power/>
                                <ci> K1 </ci>
                                <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                            </apply>
                            <apply>
                                <power/>
                                <ci> Ca_cyt </ci>
                                <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <minus/>
                        <ci> Ca_ER </ci>
                        <ci> Ca_cyt </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000018" name="Ca_leak_flux_from_the_ER">
        <variable cmeta:id="Ca_leak_flux_from_the_ER_J_leak" name="J_leak" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000017" initial_value="0.05" name="k_leak" units="first_order_rate_constant"/>
        <variable name="Ca_cyt" public_interface="in" units="micromolar"/>
        <variable name="Ca_ER" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_leak_calculation">
                <eq/>
                <ci> J_leak </ci>
                <apply>
                    <times/>
                    <ci> k_leak </ci>
                    <apply>
                        <minus/>
                        <ci> Ca_ER </ci>
                        <ci> Ca_cyt </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000025" name="mitochondrial_Ca_uptake">
        <variable cmeta:id="mitochondrial_Ca_uptake_J_in" name="J_in" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000026" initial_value="300.0" name="k_in" units="flux"/>
        <variable initial_value="0.8" name="K2" units="micromolar"/>
        <variable name="Ca_cyt" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_in_calculation">
                <eq/>
                <ci> J_in </ci>
                <apply>
                    <times/>
                    <ci> k_in </ci>
                    <apply>
                        <divide/>
                        <apply>
                            <power/>
                            <ci> Ca_cyt </ci>
                            <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 8.0 </cn>
                        </apply>
                        <apply>
                            <plus/>
                            <apply>
                                <power/>
                                <ci> K2 </ci>
                                <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 8.0 </cn>
                            </apply>
                            <apply>
                                <power/>
                                <ci> Ca_cyt </ci>
                                <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 8.0 </cn>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000027" name="mitochondrial_Ca_release">
        <variable name="J_out" public_interface="out" units="flux"/>
        <variable initial_value="125.0" name="k_out" units="first_order_rate_constant"/>
        <variable initial_value="0.00625" name="k_m" units="first_order_rate_constant"/>
        <variable initial_value="5.0" name="K3" units="micromolar"/>
        <variable name="Ca_cyt" public_interface="in" units="micromolar"/>
        <variable name="Ca_m" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_out_calculation">
                <eq/>
                <ci> J_out </ci>
                <apply>
                    <times/>
                    <apply>
                        <plus/>
                        <apply>
                            <times/>
                            <ci> k_out </ci>
                            <apply>
                                <divide/>
                                <apply>
                                    <power/>
                                    <ci> Ca_cyt </ci>
                                    <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                                </apply>
                                <apply>
                                    <plus/>
                                    <apply>
                                        <power/>
                                        <ci> K3 </ci>
                                        <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                                    </apply>
                                    <apply>
                                        <power/>
                                        <ci> Ca_cyt </ci>
                                        <cn cellml:units="dimensionless" xmlns:cellml="http://www.cellml.org/cellml/1.0#"> 2.0 </cn>
                                    </apply>
                                </apply>
                            </apply>
                        </apply>
                        <ci> k_m </ci>
                    </apply>
                    <ci> Ca_m </ci>
                </apply>
            </apply>
        </math>
    </component>
    <connection>
        <map_components component_1="cytosolic_calcium" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="ER_calcium" component_2="environment"/>
