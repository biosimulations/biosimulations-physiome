        <map_variables variable_1="beta_ER" variable_2="beta_ER"/>
        <map_variables variable_1="Ca_ER" variable_2="Ca_ER"/>
    </connection>
    <connection>
        <map_components component_1="total_calcium" component_2="mitochondrial_calcium"/>
