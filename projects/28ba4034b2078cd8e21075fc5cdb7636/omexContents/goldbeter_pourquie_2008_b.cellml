	<math xmlns="http://www.w3.org/1998/Math/MathML" id="29"> 
      <apply>
        <eq/>
	    <ci> VaX </ci>  
		<apply>
          <times/>
		  <ci> VMaX </ci>
		  <apply>
            <divide/>
			<ci> ERKa </ci>
			<ci> ERKt </ci>
		  </apply>
		  <apply>
            <divide/>
			<ci> Xi </ci>
		    <apply>
              <plus/>
			  <ci> KaX </ci>
			  <ci> Xi </ci>
			</apply>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="30"> 
      <apply>
        <eq/>
	    <ci> VdX </ci>  
		<apply>
          <times/>
		  <ci> VMdX </ci>
		  <apply>
            <divide/>
