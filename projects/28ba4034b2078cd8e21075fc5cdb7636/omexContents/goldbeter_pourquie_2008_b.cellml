	<variable units="flux" public_interface="in" name="VK"/>
	<variable units="flux" public_interface="in" name="VP"/>
	<variable units="flux" public_interface="in" name="V2"/>
	<variable units="nanomolar" public_interface="in" name="Kt"/>
