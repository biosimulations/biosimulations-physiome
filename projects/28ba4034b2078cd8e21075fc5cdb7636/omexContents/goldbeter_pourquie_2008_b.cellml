		    <apply>
              <plus/>
			  <ci> KdX </ci>
			  <ci> Xa </ci>
			</apply>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="31"> 
      <apply>
        <eq/>
	    <ci> VsMDusp </ci>  
		<apply>
          <times/>
		  <ci> VMsMDusp </ci>
		  <apply>
            <divide/>
			<apply>
              <power/>
			  <ci> Xa </ci>
			  <ci> q </ci>
			</apply>
			<apply>
              <plus/>
			  <apply>
                <power/>
			    <ci> KaMDusp </ci>
			    <ci> q </ci>
			  </apply>
			  <apply>
                <power/>
			    <ci> Xa </ci>
			    <ci> q </ci>
			  </apply>
