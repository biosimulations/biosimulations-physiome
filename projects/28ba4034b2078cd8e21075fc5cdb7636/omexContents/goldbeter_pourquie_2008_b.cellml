		  <apply>
            <divide/>
			<apply>
              <power/>
			  <ci> Fgf </ci>
			  <ci> r </ci>
			</apply>
			<apply>
              <plus/>
			  <apply>
                <power/>
			    <ci> KaFgf </ci>
