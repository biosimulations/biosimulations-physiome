	<variable name="ERKi" units="nanomolar"/>
	<variable name="Xi" units="nanomolar"/>
	<variable initial_value="2.0" name="Rast" units="nanomolar"/>
	<variable initial_value="2.0" name="ERKt" units="nanomolar"/>
	<variable initial_value="2.0" name="Xt" units="nanomolar"/>
