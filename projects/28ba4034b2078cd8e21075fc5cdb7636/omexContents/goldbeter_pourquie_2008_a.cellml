			</apply>
		  </apply>
		  <apply>
            <divide/>
            <ci> B </ci>
		    <apply>
              <plus/>
			  <ci> K1 </ci>
			  <ci> B </ci>
			</apply>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="16"> 
