        <apply>
          <times/>
          <ci> theta </ci>
		  <apply>
            <minus/>
		    <apply>
              <plus/>
			  <ci> v0 </ci>
			  <apply>
                <times/>
			    <ci> vMB </ci>
			    <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> BN </ci>
				    <ci> n </ci>
				  </apply>
				  <apply>
                    <plus/>
				    <apply>
                      <power/>
                      <ci> KaB </ci>
