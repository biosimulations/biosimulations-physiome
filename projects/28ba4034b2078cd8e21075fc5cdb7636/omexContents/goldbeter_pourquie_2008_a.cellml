        <eq/>
	    <ci> V2 </ci>  
		<apply>
          <minus/>
		  <apply>
            <times/>
			<ci> kt4 </ci>
			<ci> BN </ci>
		  </apply>
		  <apply>
            <times/>
			<ci> kt3 </ci>
			<ci> B </ci>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="15"> 
