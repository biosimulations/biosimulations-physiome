	<variable name="Nan" public_interface="in" units="nanomolar"/>
	<variable name="K" public_interface="in" units="nanomolar"/>
    <variable name="epsilon" public_interface="in" units="dimensionless"/>
	<variable name="p" public_interface="in" units="dimensionless"/>
	<variable name="time" public_interface="in" units="minute"/>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="4"> 
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>MF</ci>
        </apply> 
        <apply>
          <times/>
          <ci> epsilon </ci>
		  <apply>
            <minus/>
		    <apply>
              <times/>
			  <ci> vsFK </ci>
			  <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> Nan </ci>
			      <ci> p </ci>
			    </apply>
				<apply>
                  <plus/>
				  <apply>
                    <power/>
                    <ci> KA </ci>
					<ci> p </ci>
				  </apply>
			      <apply>
                    <power/>
                    <ci> Nan </ci>
				    <ci> p </ci>
				  </apply>
                </apply>
		      </apply>
			</apply>
			<apply>
              <times/>
			  <ci> vmF </ci>
			  <apply>
                <divide/>
                <ci> MF </ci>
				<apply>
                  <plus/>
                  <ci> KdMF </ci>
		          <ci> MF </ci>
	            </apply>
		      </apply>
			</apply>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="4a"> 
      <apply>
        <eq/>
        <ci>vsFK</ci>
        <apply>
          <times/>
          <ci> vsF </ci>
		  <apply>
            <divide/>
            <ci> KIG1 </ci>
		    <apply>
              <plus/>
              <ci> KIG1 </ci>
		      <ci> K </ci>
	        </apply>
		  </apply>
	    </apply>
	  </apply>
    </math>
  </component>
  
