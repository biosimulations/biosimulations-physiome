				    </apply>
				  </apply>
			    </apply>
			  </apply>
			  <apply>
                <times/>
			    <ci> vMXa </ci>
			    <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> Xa </ci>
				    <ci> m </ci>
				  </apply>
				  <apply>
                    <plus/>
				    <apply>
                      <power/>
                      <ci> KaXa </ci>
