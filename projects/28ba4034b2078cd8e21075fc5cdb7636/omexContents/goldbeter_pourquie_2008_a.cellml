  <component cmeta:id="N" name="N">
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
      <rdf:Description rdf:about="#N">
        <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">N</dc:title>
        <dcterms:alternative xmlns:dcterms="http://purl.org/dc/terms/">Notch protein</dcterms:alternative>  
      </rdf:Description>
    </rdf:RDF>
    <variable cmeta:id="N_N" initial_value="0.5" name="N" public_interface="out" units="nanomolar"/>
	
	<variable initial_value="1.4" name="KdN" units="nanomolar"/>
	<variable initial_value="0.23" name="vsN" units="flux"/>
	<variable initial_value="2.82" name="vdN" units="flux"/>
	
	<variable name="kc" public_interface="in" units="first_order_rate_constant"/>
	<variable name="KIF" public_interface="in" units="nanomolar"/>
    <variable name="F" public_interface="in" units="nanomolar"/>
	<variable name="epsilon" public_interface="in" units="dimensionless"/>
	<variable name="j" public_interface="in" units="dimensionless"/>
	<variable name="time" public_interface="in" units="minute"/>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="1"> 
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>N</ci>
        </apply> 
        <apply>
          <times/>
          <ci> epsilon </ci>
		  <apply>
            <minus/>
		    <ci> vsN </ci>
			<apply>
              <plus/>
              <apply>
                <times/>
			    <ci> vdN </ci>
			    <apply>
                  <divide/>
                  <ci> N </ci>
				  <apply>
                    <plus/>
                    <ci> KdN </ci>
		            <ci> N </ci>
	              </apply>
				</apply>
			  </apply>
			  <apply>
                <times/>
			    <ci> kc </ci>
				<ci> N </ci>
			    <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> KIF </ci>
					<ci> j </ci>
				  </apply>
				  <apply>
                    <plus/>
				    <apply>
                      <power/>
                      <ci> KIF </ci>
					  <ci> j </ci>
				    </apply>
					<apply>
                      <power/>
                      <ci> F </ci>
					  <ci> j </ci>
				    </apply>
                  </apply>
				</apply>
			  </apply>
	        </apply>
		  </apply>
	    </apply>
      </apply>
    </math>
  </component>
  
  <component cmeta:id="Na" name="Na">
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
      <rdf:Description rdf:about="#Na">
        <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">Na</dc:title>
        <dcterms:alternative xmlns:dcterms="http://purl.org/dc/terms/">cytosolic active Notch protein</dcterms:alternative>  
      </rdf:Description>
    </rdf:RDF>
    <variable initial_value="0.2" name="Na" public_interface="out" units="nanomolar"/>
	<variable name="Vtr" public_interface="out" units="flux"/>
	
	<variable initial_value="0.001" name="KdNa" units="nanomolar"/>
	<variable initial_value="0.01" name="VdNa" units="flux"/>
	<variable initial_value="0.1" name="kt1" units="first_order_rate_constant"/>
	<variable initial_value="0.1" name="kt2" units="first_order_rate_constant"/>
	
	<variable name="kc" public_interface="in" units="first_order_rate_constant"/>
	<variable name="N" public_interface="in" units="nanomolar"/>
	<variable name="Nan" public_interface="in" units="nanomolar"/>
    <variable name="KIF" public_interface="in" units="nanomolar"/>
    <variable name="F" public_interface="in" units="nanomolar"/>
	<variable name="epsilon" public_interface="in" units="dimensionless"/>
	<variable name="j" public_interface="in" units="dimensionless"/>
	<variable name="time" public_interface="in" units="minute"/>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="2"> 
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Na</ci>
        </apply> 
        <apply>
          <times/>
          <ci> epsilon </ci>
		  <apply>
            <minus/>
		    <apply>
              <times/>
			  <ci> kc </ci>
			  <ci> N </ci>
			  <apply>
                <divide/>
                <apply>
                  <power/>
                  <ci> KIF </ci>
			      <ci> j </ci>
			    </apply>
				<apply>
                  <plus/>
				  <apply>
                    <power/>
                    <ci> KIF </ci>
					<ci> j </ci>
				  </apply>
			      <apply>
                    <power/>
                    <ci> F </ci>
				    <ci> j </ci>
				  </apply>
                </apply>
		      </apply>
			</apply>
			<apply>
              <plus/>
              <apply>
                <times/>
			    <ci> VdNa </ci>
			    <apply>
                  <divide/>
                  <ci> Na </ci>
				  <apply>
                    <plus/>
                    <ci> KdNa </ci>
		            <ci> Na </ci>
	              </apply>
				</apply>
			  </apply>
			  <ci> Vtr </ci>
		    </apply>
		  </apply>
	    </apply>
      </apply>
    </math>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML" id="2a"> 
      <apply>
        <eq/>
        <ci>Vtr</ci>
        <apply>
          <minus/>
		  <apply>
            <times/>
	        <ci> kt1 </ci>
		    <ci> Na </ci>
		  </apply>
		  <apply>
            <times/>
	        <ci> kt2 </ci>
		    <ci> Nan </ci>
		  </apply>
	    </apply>
      </apply>
    </math>
  </component>
  
