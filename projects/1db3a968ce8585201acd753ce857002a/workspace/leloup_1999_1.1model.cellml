				  <ci> k3 </ci>
				  <ci> P2 </ci>
				  <ci> T2 </ci>
				</apply>
				<apply>
				  <times/>
				  <ci> k2 </ci>
				  <ci> CN </ci>
				</apply>
			  </apply>
			  <apply>
				<plus/>
				<apply>
				  <times/>
				  <ci> k4 </ci>
				  <ci> C </ci>
				</apply>
				<apply>
				  <times/>
				  <ci> k1 </ci>
				  <ci> C </ci>
				</apply>
				<apply>
				  <times/>
				  <ci> kdC </ci>
				  <ci> C </ci>
				</apply>
			  </apply>
			</apply>
		  </apply>    
		</math>
