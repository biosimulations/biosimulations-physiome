        <variable name="JGPDH" public_interface="in" units="micromolar_millisecond"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci> JPDH </ci>
                <apply>
                    <times/>
                    <apply>
                        <divide/>
                        <ci> p1 </ci>
                        <apply>
                            <plus/>
                            <ci> p2 </ci>
                            <apply>
                                <divide/>
                                <ci> NADHm </ci>
                                <ci> NADm </ci>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <divide/>
                        <ci> Cam </ci>
                        <apply>
                            <plus/>
                            <ci> p3 </ci>
                            <ci> Cam </ci>
                        </apply>
                    </apply>
                    <apply>
                        <plus/>
