    <component name="membrane">
        <variable cmeta:id="membrane_Vm" initial_value="-60.0" name="Vm" public_interface="out" units="millivolt"/>
        <variable initial_value="5300.0" name="cm" units="femtofarad"/>
        <variable name="Ica" public_interface="in" units="femtoampere"/>
        <variable name="Ik" public_interface="in" units="femtoampere"/>
        <variable name="Ikca" public_interface="in" units="femtoampere"/>
        <variable name="Ikatp" public_interface="in" units="femtoampere"/>
        <variable name="time" public_interface="in" units="millisecond"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci> Vm </ci>
                </apply>
                <apply>
                    <divide/>
                    <apply>
                        <minus/>
                        <apply>
                            <plus/>
                            <ci> Ik </ci>
                            <ci> Ica </ci>
                            <ci> Ikca </ci>
                            <ci> Ikatp </ci>
                        </apply>
                    </apply>
                    <ci> cm </ci>
                </apply>
            </apply>
        </math>
