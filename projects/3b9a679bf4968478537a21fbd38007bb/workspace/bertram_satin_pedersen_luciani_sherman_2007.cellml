    <component cmeta:id="Ikca" name="Ikca">
        
        <variable name="Ikca" public_interface="out" units="femtoampere"/>
        <variable initial_value="300.0" name="gkCa" units="picosiemens"/>
        <variable initial_value="0.5" name="kd" units="micromolar"/>
        <variable name="time" public_interface="in" units="millisecond"/>
        <variable name="VK" public_interface="in" units="millivolt"/>
        <variable name="c" public_interface="in" units="micromolar"/>
        <variable name="Vm" public_interface="in" units="millivolt"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci> Ikca </ci>
                <apply>
                    <times/>
                    <apply>
                        <divide/>
                        <ci> gkCa </ci>
                        <apply>
                            <plus/>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                            <apply>
                                <power/>
                                <apply>
                                    <divide/>
                                    <ci> kd </ci>
                                    <ci> c </ci>
                                </apply>
                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 2.0 </cn>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <minus/>
                        <ci> Vm </ci>
                        <ci> VK </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="Ikatp" name="Ikatp">
        
        <variable name="Ikatp" public_interface="out" units="femtoampere"/>
        <variable initial_value="16000.0" name="gkATP_" units="picosiemens"/>
        <variable name="katpo" units="dimensionless"/>
        <variable name="topo" units="dimensionless"/>
        <variable name="bottomo" units="dimensionless"/>
        <variable name="mgadp" units="micromolar"/>
        <variable name="adp3m" units="micromolar"/>
        <variable name="atp4m" units="micromolar"/>
        <variable name="time" public_interface="in" units="millisecond"/>
        <variable name="VK" public_interface="in" units="millivolt"/>
        <variable name="atp" public_interface="in" units="micromolar"/>
        <variable name="adp" public_interface="in" units="micromolar"/>
        <variable name="Vm" public_interface="in" units="millivolt"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci> Ikatp </ci>
                <apply>
                    <times/>
                    <ci> gkATP_ </ci>
                    <ci> katpo </ci>
                    <apply>
                        <minus/>
                        <ci> Vm </ci>
                        <ci> VK </ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> katpo </ci>
                <apply>
                    <divide/>
                    <ci>topo</ci>
                    <ci>bottomo</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> topo </ci>
                <apply>
                    <plus/>
                    <apply>
                        <times/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.08 </cn>
                        <apply>
                            <plus/>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                            <apply>
                                <divide/>
                                <apply>
                                    <times/>
                                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 2.0 </cn>
                                    <ci> mgadp </ci>
                                </apply>
                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar"> 17.0 </cn>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <times/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.89 </cn>
                        <apply>
                            <power/>
                            <apply>
                                <divide/>
                                <ci> mgadp </ci>
                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar"> 17.0 </cn>
                            </apply>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 2.0 </cn>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> bottomo </ci>
                <apply>
                    <times/>
                    <apply>
                        <power/>
                        <apply>
                            <plus/>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                            <apply>
                                <divide/>
                                <ci> mgadp </ci>
                                <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar"> 17.0 </cn>
                            </apply>
                        </apply>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 2.0 </cn>
                    </apply>
                    <apply>
                        <plus/>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                        <apply>
                            <divide/>
                            <ci> adp3m </ci>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar"> 26.0 </cn>
                        </apply>
                        <apply>
                            <divide/>
                            <ci> atp4m </ci>
                            <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="micromolar"> 1.0 </cn>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> mgadp </ci>
                <apply>
                    <times/>
                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.165 </cn>
                    <ci> adp </ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> adp3m </ci>
                <apply>
                    <times/>
                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.135 </cn>
                    <ci> adp </ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci> atp4m </ci>
                <apply>
                    <times/>
                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 0.05 </cn>
                    <ci> atp </ci>
                </apply>
            </apply>
        </math>
    </component>
    <!-- The glycolytic model -->
