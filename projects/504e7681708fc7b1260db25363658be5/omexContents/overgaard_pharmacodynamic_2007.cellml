   <component name="model_parameters">
      <variable initial_value="21.0" name="T_a" public_interface="out" units="celsius"/>
	  <variable initial_value="38.0" name="T_b" public_interface="out" units="celsius"/>
	  <variable initial_value="1.57" name="delta_T" public_interface="out" units="celsius"/>
	  <variable initial_value="0.0258" name="kinc" public_interface="out" units="W_per_kg_C2"/>
	  <variable initial_value="24.0" name="tdose1" public_interface="out" units="hour"/>
	  <variable initial_value="72.0" name="tdose2" public_interface="out" units="hour"/>
	  <variable initial_value="120.0" name="tdose3" public_interface="out" units="hour"/>
   </component>
   
   <component name="M_c">
      <variable name="M_c" public_interface="out" units="W_per_kg"/>
      
	  <variable initial_value="17.5" name="t_day" units="hour"/>
	  <variable initial_value="6.73" name="t_night" units="hour"/>
	  <variable name="tprime" units="second"/>
      <variable initial_value="86400" name="day_length" units="second"/>
	  
	  <variable name="M_day" public_interface="in" units="W_per_kg"/>
	  <variable name="M_night" public_interface="in" units="W_per_kg"/>
	  <variable name="time" public_interface="in" units="hour"/>
      
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="1">
         <apply>
		    <eq/>
			<ci>tprime</ci>
			<apply>
			   <rem/>
			   <apply>
			      <times/>
				  <ci>time</ci>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="per_hour">3600</cn>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="second">1</cn>
			   </apply>
			   <ci>day_length</ci>
			</apply>
		 </apply>
	  </math>
      
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="1a">
         <apply>
            <eq/>
            <ci>M_c</ci>
            <piecewise>
               <piece>
                  <ci>M_night</ci>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <apply>
                           <divide/>
                           <ci>tprime</ci>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="seconds_per_hour">3600</cn>
                        </apply>
                        <ci>t_night</ci>
                     </apply>
                     <apply>
                        <lt/>
                        <apply>
                           <divide/>
                           <ci>tprime</ci>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="seconds_per_hour">3600</cn>
                        </apply>
                        <ci>t_day</ci>
                     </apply>
                  </apply>
               </piece>
               <otherwise>
                  <ci>M_day</ci>
               </otherwise>
            </piecewise>
         </apply>
      </math>
   </component>
   
   <component name="M">
      <variable cmeta:id="M_M" initial_value="3.5" name="M" public_interface="out" units="W_per_kg"/>
	  
	  <variable initial_value="1.1375" name="km" units="per_hour"/>
      
	  <variable name="M_c" public_interface="in" units="W_per_kg"/>
	  <variable name="time" public_interface="in" units="hour"/>
      
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="2">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>M</ci>
            </apply>
            <apply>
               <times/>
               <apply>
                  <minus/>
				  <ci>km</ci>
			   </apply>
			   <apply>
                  <minus/>
				  <ci>M</ci>
				  <ci>M_c</ci>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="T">
      <variable cmeta:id="T_T" initial_value="38.785" name="T" public_interface="out" units="celsius"/>
	  
	  <variable initial_value="3.47" name="c" units="kJ_per_kg_C"/>
      
	  <variable name="k" public_interface="in" units="W_per_kg_C"/>
	  <variable name="M" public_interface="in" units="W_per_kg"/>
	  <variable name="T_a" public_interface="in" units="celsius"/>
	  <variable name="time" public_interface="in" units="hour"/>
      
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="3">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>T</ci>
            </apply>
            <apply>
               <times/>
               <apply>
                  <power/>
				  <ci>c</ci>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">-1</cn>
			   </apply>
			   <apply>
                  <minus/>
				  <ci>M</ci>
				  <apply>
                     <times/>
				     <ci>k</ci>
					 <apply>
                        <minus/>
				        <ci>T</ci>
						<ci>T_a</ci>
					 </apply>
				  </apply>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="k">
      <variable name="k" public_interface="out" units="W_per_kg_C"/>
	  
	  <variable initial_value="0.0" name="BR" units="dimensionless"/>
	  <variable initial_value="0.144" name="pEtot" units="dimensionless"/>
	  <variable initial_value="5.35" name="kR" units="per_day"/>
	  <variable initial_value="3.0" name="AMT_dose" units="mg_per_kg"/>
	  <variable initial_value="1.0" name="pEf1" units="per_day"/>
	  <variable initial_value="0.2" name="pEs1" units="kg_per_day_mg"/>
	  <variable initial_value="3.57" name="pEf2" units="per_day"/>
	  <variable initial_value="2.43" name="pEs2" units="kg_per_day_mg"/>
	  <variable initial_value="8.0" name="pEf3" units="per_day"/>
	  <variable initial_value="50.0" name="pEs3" units="kg_per_day_mg"/>
	  <variable cmeta:id="k_E_slow" name="E_slow" units="per_day"/>
	  <variable cmeta:id="k_E_fast" name="E_fast" units="per_day"/>
	  <variable name="f2_drug" units="W_per_kg_C"/>
	  
	  <variable name="kinc" public_interface="in" units="W_per_kg_C2"/>
      <variable name="kb" public_interface="in" units="W_per_kg_C"/>
	  <variable name="T" public_interface="in" units="celsius"/>
	  <variable name="T_b" public_interface="in" units="celsius"/>
	  <variable name="f_prime" public_interface="in" units="dimensionless"/>
	  <variable name="gNsTs1" public_interface="in" units="dimensionless"/>
	  <variable name="gNsTs2" public_interface="in" units="dimensionless"/>
	  <variable name="gNsTs3" public_interface="in" units="dimensionless"/>
	  <variable name="gNfTf1" public_interface="in" units="dimensionless"/>
	  <variable name="gNfTf2" public_interface="in" units="dimensionless"/>
	  <variable name="gNfTf3" public_interface="in" units="dimensionless"/>
	  <variable name="time" public_interface="in" units="hour"/>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="4">
         <apply>
            <eq/>
            <ci>k</ci>
            <apply>
               <plus/>
               <ci>kb</ci>
			   <apply>
                  <times/>
				  <ci>kinc</ci>
				  <apply>
                     <minus/>
				     <ci>T</ci>
					 <apply>
                        <times/>
					    <ci>T_b</ci>
						<apply>
                           <plus/>
						   <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
						   <apply>
                              <times/>
					          <ci>pEtot</ci>
							  <ci>BR</ci>
						   </apply>
						</apply>
				     </apply>
			      </apply>
			   </apply>
			   <ci>f2_drug</ci>
            </apply>
         </apply>
      </math>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="4a">
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>BR</ci>
            </apply>
            <apply>
               <minus/>
               <apply>
                  <times/>
				  <apply>
                     <times/>
				     <ci>f_prime</ci>
					 <apply>
                        <plus/>
				        <ci>E_slow</ci>
						<ci>E_fast</ci>
					 </apply>
				  </apply>
				  <apply>
                     <minus/>
					 <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
					 <ci>BR</ci>
				  </apply>
			   </apply>
			   <apply>
                  <times/>
				  <ci>kR</ci>
				  <ci>BR</ci>
			   </apply>
            </apply>
         </apply>
      </math>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="4b">
         <apply>
            <eq/>
            <ci>E_slow</ci>
            <apply>
               <times/>
               <ci>AMT_dose</ci>
               <ci>pEs2</ci>
               <apply>
                  <plus/>
                  <ci>gNsTs1</ci>
                  <ci>gNsTs2</ci>
                  <ci>gNsTs3</ci>
               </apply>
            </apply>
         </apply>
	  </math>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="4c"> 
		 <apply>
            <eq/>
            <ci>E_fast</ci>
            <apply>
               <times/>
               <ci>pEf2</ci>
               <apply>
                  <plus/>
                  <ci>gNfTf1</ci>
                  <ci>gNfTf2</ci>
                  <ci>gNfTf3</ci>
               </apply>
            </apply>
         </apply>
	  </math> 
	
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="4d">
         <apply>
            <eq/>
            <ci>f2_drug</ci>
			<cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="W_per_kg_C">0</cn>
	     </apply>
      </math>
   </component>
   
   <component name="T_day">
      <variable name="T_day" public_interface="out" units="celsius"/>
    
	  <variable name="T_b" public_interface="in" units="celsius"/>
      <variable name="delta_T" public_interface="in" units="celsius"/>
	
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="5"> 
         <apply>
            <eq/>
            <ci>T_day</ci>
            <apply>
               <plus/>
               <ci>T_b</ci>
			   <apply>
                  <divide/>
				  <ci>delta_T</ci>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="T_night">
      <variable name="T_night" public_interface="out" units="celsius"/>
    
	  <variable name="T_b" public_interface="in" units="celsius"/>
      <variable name="delta_T" public_interface="in" units="celsius"/>
	
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="6"> 
         <apply>
            <eq/>
            <ci>T_night</ci>
            <apply>
               <minus/>
               <ci>T_b</ci>
			   <apply>
                  <divide/>
				  <ci>delta_T</ci>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2</cn>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="kb">
      <variable name="kb" public_interface="out" units="W_per_kg_C"/>
    
	  <variable initial_value="3.0" name="M_b" units="W_per_kg"/>
	
	  <variable name="T_b" public_interface="in" units="celsius"/>
      <variable name="T_a" public_interface="in" units="celsius"/>
	
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="7"> 
         <apply>
            <eq/>
            <ci>kb</ci>
            <apply>
               <divide/>
               <ci>M_b</ci>
			   <apply>
                  <minus/>
				  <ci>T_b</ci>
				  <ci>T_a</ci>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="M_day">
      <variable name="M_day" public_interface="out" units="W_per_kg"/>
    
	  <variable name="kinc" public_interface="in" units="W_per_kg_C2"/>
      <variable name="kb" public_interface="in" units="W_per_kg_C"/>
	  <variable name="T_b" public_interface="in" units="celsius"/>
      <variable name="T_a" public_interface="in" units="celsius"/>
	  <variable name="T_day" public_interface="in" units="celsius"/>
	
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="8"> 
         <apply>
            <eq/>
            <ci>M_day</ci>
            <apply>
               <times/>
			   <apply>
                  <plus/>
                  <ci>kb</ci>
				  <apply>
                     <times/>
					 <ci>kinc</ci>
			         <apply>
                        <minus/>
				        <ci>T_day</ci>
						<ci>T_b</ci>
				     </apply>
				  </apply>
			   </apply>
			   <apply>
                  <minus/>
				  <ci>T_day</ci>
			      <ci>T_a</ci>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="M_night">
      <variable name="M_night" public_interface="out" units="W_per_kg"/>
	  <variable cmeta:id="M_night_f_prime" name="f_prime" public_interface="out" units="dimensionless"/>
	  
	  <variable initial_value="45.12" name="t_prime" units="hour"/>
	  <variable initial_value="0.2229166" name="alpha" units="per_hour"/>
	  <variable initial_value="1.0" name="delta_high_dose" units="dimensionless"/>
	  <variable name="M_night_baseline" units="W_per_kg"/>
      
	  <variable name="tdose1" public_interface="in" units="hour"/>
	  <variable name="kinc" public_interface="in" units="W_per_kg_C2"/>
      <variable name="kb" public_interface="in" units="W_per_kg_C"/>
	  <variable name="T_b" public_interface="in" units="celsius"/>
      <variable name="T_a" public_interface="in" units="celsius"/>
	  <variable name="T_night" public_interface="in" units="celsius"/>
	  <variable name="M_day" public_interface="in" units="W_per_kg"/>
	  <variable name="time" public_interface="in" units="hour"/>
	
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="9"> 
         <apply>
            <eq/>
            <ci>f_prime</ci>
            <apply>
               <times/>
			   <ci>delta_high_dose</ci>
			   <apply>
                  <power/>
				  <apply>
                     <plus/>
					 <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
					 <apply>
                        <exp/>
						<apply>
                           <times/>
						   <apply>
                              <minus/>
                              <ci>alpha</ci>
						   </apply>
						   <apply>
                              <minus/>
                              <ci>time</ci>
				              <apply>
                                 <plus/>
				                 <ci>tdose1</ci>
						         <ci>t_prime</ci>
				              </apply>
				           </apply>
			            </apply>
				     </apply>
			      </apply>
				  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">-1</cn>
			   </apply>
            </apply>
         </apply>
      </math>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="9a"> 
         <apply>
            <eq/>
            <ci>M_night</ci>
            <apply>
               <plus/>
			   <apply>
                  <times/>
                  <apply>
                     <minus/>
					 <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
					 <ci>f_prime</ci>
			      </apply>
				  <ci>M_night_baseline</ci>
			   </apply>
			   <apply>
                  <times/>
				  <ci>f_prime</ci>
			      <ci>M_day</ci>
			   </apply>
            </apply>
         </apply>
      </math>
	  
	  <math xmlns="http://www.w3.org/1998/Math/MathML" id="9b"> 
         <apply>
            <eq/>
            <ci>M_night_baseline</ci>
            <apply>
               <times/>
			   <apply>
                  <plus/>
                  <ci>kb</ci>
				  <apply>
                     <times/>
					 <ci>kinc</ci>
			         <apply>
                        <minus/>
				        <ci>T_night</ci>
						<ci>T_b</ci>
				     </apply>
				  </apply>
			   </apply>
			   <apply>
                  <minus/>
				  <ci>T_night</ci>
			      <ci>T_a</ci>
			   </apply>
            </apply>
         </apply>
      </math>
   </component>
   
   <component name="gNT">
      <variable name="gNsTs1" public_interface="out" units="dimensionless"/>
	  <variable name="gNsTs2" public_interface="out" units="dimensionless"/>
	  <variable name="gNsTs3" public_interface="out" units="dimensionless"/>
	  <variable name="gNfTf1" public_interface="out" units="dimensionless"/>
	  <variable name="gNfTf2" public_interface="out" units="dimensionless"/>
	  <variable name="gNfTf3" public_interface="out" units="dimensionless"/>
	  
	  <variable initial_value="4.0" name="Ns" units="dimensionless"/>
	  <variable initial_value="4.0" name="Nf" units="dimensionless"/>
	  <variable initial_value="2.45" name="Ts" units="day"/>
	  <variable initial_value="0.368" name="Tf" units="day"/>
	  <variable name="X1" units="day"/>
	  <variable name="X2" units="day"/>
	  <variable name="X3" units="day"/>
	  <variable name="Kf" units="per_day"/>
	  <variable name="Ks" units="per_day"/>
	  
	  <variable name="tdose1" public_interface="in" units="hour"/>
	  <variable name="tdose2" public_interface="in" units="hour"/>
	  <variable name="tdose3" public_interface="in" units="hour"/>
	  <variable name="time" public_interface="in" units="hour"/>
	  
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="10"> 
         <apply>
            <eq/>
            <ci>gNsTs1</ci>
           <piecewise>
               <piece>
