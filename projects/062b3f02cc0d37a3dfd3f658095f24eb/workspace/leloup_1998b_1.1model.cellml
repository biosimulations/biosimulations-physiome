		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply>
				<eq/>
				<apply>
					<diff/>
					<bvar>
						<ci>time</ci>
					</bvar>
					<ci>C</ci>
				</apply>
				<apply>
					<minus/>
					<apply>
						<plus/>
						<apply>
							<minus/>
							<apply>
								<minus/>
								<apply>
									<times/>
									<ci>k_3</ci>
									<ci>P_2</ci>
									<ci>T_2</ci>
								</apply>
								<apply>
									<times/>
									<ci>k_4</ci>
									<ci>C</ci>
								</apply>
							</apply>
							<apply>
								<times/>
								<ci>k_1</ci>
								<ci>C</ci>
							</apply>
						</apply>
						<apply>
							<times/>
							<ci>k_2</ci>
							<ci>C_N</ci>
						</apply>
					</apply>
					<apply>
						<times/>
						<ci>k_dC</ci>
						<ci>C</ci>
