    <math xmlns="http://www.w3.org/1998/Math/MathML" id="35">
      <apply id="v_ADH_calculation"><eq/>
	    <ci> v_ADH </ci>
        <apply><minus/>
		  <apply><times/>
		    <apply><divide/>
			  <ci> v_max </ci>
			  <apply><times/>
                <ci> Ki_NAD </ci>
                <ci> Km_ETOH </ci>
