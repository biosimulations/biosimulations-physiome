   </units>
   
   <units name="nM">
         <unit prefix="nano" units="mole"/>
         <unit exponent="-1" units="liter"/>
      </units>
   
   <!--
     *********************************************************
     ***       C   O   M   P   O   N   E   N   T   S       ***
     *********************************************************
