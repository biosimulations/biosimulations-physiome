		<component component_ref="interface" name="V_GAPDHi"/>
	</import>
	<import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="cons_law1.cellml">
		<component component_ref="interface" name="V_GKi"/>
	</import>
	<import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="cons_law1.cellml">
		<component component_ref="interface" name="V_PFKi"/>
	</import>
	<import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="cons_law1.cellml">
		<component component_ref="interface" name="V_FBAi"/>
	</import>
	<import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="cons_law2.cellml">
