                </apply>
              </apply>
              <ci> DA </ci>
            </apply>
            <apply>
              <times/>
              <ci> gamma_Fdmax </ci>
              <apply>
                <minus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <power/>
                    <ci> Fd </ci>
                    <cn cellml:units="dimensionless"> 2.0 </cn> 
