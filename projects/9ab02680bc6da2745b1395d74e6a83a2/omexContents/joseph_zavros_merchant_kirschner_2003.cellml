                </apply>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <ci> AC </ci>
                    <cn cellml:units="dimensionless"> 2.0 </cn> 
                  </apply>
                  <apply>
                    <power/>
                    <ci> alpha_HA </ci>
                    <cn cellml:units="dimensionless"> 2.0 </cn> 
