    <!--
    Below, we define some additional units for association with variables and
    constants within the model. The identifiers are fairly self-explanatory.
  -->
    <units name="micromolar">
        <unit prefix="micro" units="mole"/>
        <unit exponent="-1" units="litre"/>
    </units>
    <units name="flux">
        <unit units="micromolar"/>
        <unit exponent="-1" units="second"/>
    </units>
    <units name="mm2_per_second">
        <unit exponent="2" prefix="micro" units="metre"/>
        <unit exponent="-1" units="second"/>
    </units>
    <!--
    The "environment" component is used to declare variables that are used by
    all or most of the other components, in this case just "time".
  -->
    <component name="environment">
        <variable name="time" public_interface="out" units="second"/>
    </component>
    <component cmeta:id="id_000000001" name="IP3">
        <variable initial_value="0.0" name="P" public_interface="out" units="micromolar"/>
        <variable initial_value="300.0" name="D_P" units="mm2_per_second"/>
        <variable name="Pxx" units="micromolar"/>
        <variable name="Pyy" units="micromolar"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="P_partialdiff_eq">
                <eq/>
                <apply>
                    <partialdiff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> P </ci>
                </apply>
                <apply>
                    <times/>
                    <ci> D_P </ci>
                    <apply>
                        <plus/>
                        <ci> Pxx </ci>
                        <ci> Pyy </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component name="calcium">
        <variable initial_value="0.3" name="c" public_interface="out" units="micromolar"/>
        <variable initial_value="20.0" name="D_c" units="mm2_per_second"/>
        <variable name="cxx" units="micromolar"/>
        <variable name="cyy" units="micromolar"/>
        <variable name="time" public_interface="in" units="second"/>
        <variable name="J_channel" public_interface="in" units="flux"/>
        <variable name="J_pump" public_interface="in" units="flux"/>
        <variable name="J_leak" public_interface="in" units="flux"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="c_partialdiff_eq">
                <eq/>
                <apply>
                    <partialdiff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> c </ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <times/>
                        <ci> D_c </ci>
                        <apply>
                            <plus/>
                            <ci> cxx </ci>
                            <ci> cyy </ci>
                        </apply>
                    </apply>
                    <ci> J_channel </ci>
                    <ci> J_leak </ci>
                    <apply>
                        <minus/>
                        <ci> J_pump </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000002" name="IP3_receptor">
        <variable cmeta:id="id_000000004" name="J_channel" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000005" initial_value="16.0" name="k_flux" units="flux"/>
        <variable cmeta:id="id_000000006" name="mu_P" units="dimensionless"/>
        <variable cmeta:id="id_000000007" initial_value="0.29" name="mu_0" units="dimensionless"/>
        <variable cmeta:id="id_000000008" initial_value="0.30" name="mu_1" units="dimensionless"/>
        <variable cmeta:id="id_000000009" initial_value="0.7" name="k1" units="micromolar"/>
        <variable cmeta:id="id_000000010" initial_value="4.0" name="k_mu" units="micromolar"/>
        <variable cmeta:id="id_000000011" initial_value="0.111" name="b" units="dimensionless"/>
        <variable name="time" private_interface="out" public_interface="in" units="second"/>
        <variable name="c" private_interface="out" public_interface="in" units="micromolar"/>
        <variable name="P" public_interface="in" units="micromolar"/>
        <variable name="h" private_interface="in" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_channel_calculation">
                <eq/>
                <ci> J_channel </ci>
                <apply>
                    <times/>
                    <ci> k_flux </ci>
                    <ci> mu_P </ci>
                    <ci> h </ci>
                    <apply>
                        <plus/>
                        <ci> b </ci>
                        <apply>
                            <divide/>
                            <apply>
                                <times/>
                                <apply>
                                    <minus/>
                                    <cn cellml:units="dimensionless"> 1.0 </cn>
                                    <ci> b </ci>
                                </apply>
                                <ci> c </ci>
                            </apply>
                            <apply>
                                <plus/>
                                <ci> k1 </ci>
                                <ci> c </ci>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply id="mu_P_calculation">
                <eq/>
                <ci> mu_P </ci>
                <apply>
                    <plus/>
                    <ci> mu_0 </ci>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci> mu_1 </ci>
                            <ci> P </ci>
                        </apply>
                        <apply>
                            <plus/>
                            <ci> k_mu </ci>
                            <ci> P </ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000003" name="IP3_receptor_h_gate">
        <variable cmeta:id="id_000000012" name="h" public_interface="out" units="dimensionless"/>
        <variable cmeta:id="id_000000013" initial_value="2.0" name="tau_h" units="second"/>
        <variable cmeta:id="id_000000014" name="h_infinity" units="dimensionless"/>
        <variable cmeta:id="id_000000015" initial_value="0.7" name="k2" units="micromolar"/>
        <variable cmeta:id="id_000000016" name="c" public_interface="in" units="micromolar"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="h_diff_eq">
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci> time </ci>
                    </bvar>
                    <ci> h </ci>
                </apply>
                <apply>
                    <divide/>
                    <apply>
                        <minus/>
                        <ci> h_infinity </ci>
                        <ci> h </ci>
                    </apply>
                    <ci> tau_h </ci>
                </apply>
            </apply>
            <apply id="h_infinity_calculation">
                <eq/>
                <ci> h_infinity </ci>
                <apply>
                    <divide/>
                    <apply>
                        <power/>
                        <ci> k2 </ci>
                        <cn cellml:units="dimensionless"> 2.0 </cn>
                    </apply>
                    <apply>
                        <plus/>
                        <apply>
                            <power/>
                            <ci> k2 </ci>
                            <cn cellml:units="dimensionless"> 2.0 </cn>
                        </apply>
                        <apply>
                            <power/>
                            <ci> c </ci>
                            <cn cellml:units="dimensionless"> 2.0 </cn>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000017" name="calcium_pump">
        <variable cmeta:id="id_000000018" name="J_pump" public_interface="out" units="flux"/>
        <variable cmeta:id="id_000000019" initial_value="2.0" name="gamma" units="flux"/>
        <variable cmeta:id="id_000000020" initial_value="0.10" name="k_gamma" units="micromolar"/>
        <variable name="time" public_interface="in" units="second"/>
        <variable name="c" public_interface="in" units="micromolar"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_pump_calculation">
                <eq/>
                <ci> J_pump </ci>
                <apply>
                    <divide/>
                    <apply>
                        <times/>
                        <ci> gamma </ci>
                        <ci> c </ci>
                    </apply>
                    <apply>
                        <plus/>
                        <ci> k_gamma </ci>
                        <ci> c </ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <component cmeta:id="id_000000021" name="calcium_leak">
        <variable cmeta:id="id_000000022" name="J_leak" public_interface="out" units="flux"/>
        <variable initial_value="0.0" name="beta" units="dimensionless"/>
        <variable name="time" public_interface="in" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="J_leak_calculation">
                <eq/>
                <ci> J_leak </ci>
                <ci> beta </ci>
            </apply>
        </math>
    </component>
    <group>
        <relationship_ref relationship="encapsulation"/>
        <component_ref component="IP3_receptor">
            <component_ref component="IP3_receptor_h_gate"/>
        </component_ref>
    </group>
    <connection>
        <map_components component_1="IP3" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="calcium" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="IP3_receptor" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="calcium_pump" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="calcium_leak" component_2="environment"/>
        <map_variables variable_1="time" variable_2="time"/>
    </connection>
    <connection>
        <map_components component_1="IP3" component_2="IP3_receptor"/>
        <map_variables variable_1="P" variable_2="P"/>
    </connection>
    <connection>
        <map_components component_1="calcium" component_2="IP3_receptor"/>
        <map_variables variable_1="c" variable_2="c"/>
        <map_variables variable_1="J_channel" variable_2="J_channel"/>
    </connection>
    <connection>
        <map_components component_1="calcium" component_2="calcium_pump"/>
        <map_variables variable_1="c" variable_2="c"/>
        <map_variables variable_1="J_pump" variable_2="J_pump"/>
    </connection>
    <connection>
        <map_components component_1="calcium" component_2="calcium_leak"/>
        <map_variables variable_1="J_leak" variable_2="J_leak"/>
    </connection>
    <connection>
        <map_components component_1="IP3_receptor" component_2="IP3_receptor_h_gate"/>
        <map_variables variable_1="h" variable_2="h"/>
        <map_variables variable_1="time" variable_2="time"/>
        <map_variables variable_1="c" variable_2="c"/>
    </connection>
