                            </apply>
                            <apply>
                                <leq/>
                                <ci>PLA1</ci>
                                <cn cellml:units="mmHg">1</cn>
                            </apply>
                        </apply>
                    </piece>
                    <piece>
                        <apply>
                            <plus/>
                            <cn cellml:units="L_per_minute">3.6</cn>
                            <apply>
                                <divide/>
                                <apply>
                                    <times/>
                                    <apply>
                                        <minus/>
                                        <cn cellml:units="L_per_minute_per_mmHg">9.4</cn>
                                        <cn cellml:units="L_per_minute_per_mmHg">3.6</cn>
                                    </apply>
                                    <apply>
                                        <minus/>
                                        <ci>PLA1</ci>
                                        <cn cellml:units="mmHg">1</cn>
