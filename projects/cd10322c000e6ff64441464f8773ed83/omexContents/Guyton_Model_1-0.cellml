<!-- Parameters from parameter_file -->
		<variable name="POR"	units="mmHg" private_interface="none" public_interface="in"/>

		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply id="ARN1">
				<eq/>
				<ci>POD</ci>
				<apply>
					<minus/>
