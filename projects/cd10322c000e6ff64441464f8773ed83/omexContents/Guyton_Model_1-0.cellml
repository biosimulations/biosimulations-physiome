		<map_variables variable_1="ANMAL" variable_2="ANMAL"/>
	</connection>
	<connection>
		<map_components component_1="aldosterone_secretion" component_2="osmotic_control_of_aldosterone_secretion"/>
		<map_variables variable_1="OSMAL" variable_2="OSMAL"/>
	</connection>

