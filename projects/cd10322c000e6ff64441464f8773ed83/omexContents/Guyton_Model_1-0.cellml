		<variable name="RMULT1"	units="dimensionless" private_interface="none" public_interface="in"/>

		<math xmlns="http://www.w3.org/1998/Math/MathML">
			<apply id="CD48">
				<eq/>
				<ci>RSM</ci>
				<apply>
					<times/>
					<ci>RAM</ci>
					<ci>AMM</ci>
					<ci>R1</ci>
					<ci>MYOGRS</ci>
					<ci>RMULT1</ci>
				</apply>
			</apply>
		</math>
