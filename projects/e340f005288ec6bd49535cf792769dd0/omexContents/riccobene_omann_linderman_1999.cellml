    <variable units="molar" public_interface="in" name="LR"/>
    <variable units="molar" public_interface="in" name="R_star"/>
    <variable units="second_order_rate_constant" public_interface="in" name="kf"/> 
    <variable units="first_order_rate_constant" public_interface="in" name="kr"/>
    <variable units="first_order_rate_constant" public_interface="in" name="kfR"/> 
    <variable units="dimensionless" public_interface="in" name="Kact"/> 
    <variable units="second" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="1"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci> time </ci>
          </bvar> 
          <ci> R </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> kr </ci>
              <ci> LR </ci>
            </apply>
            <apply>
              <times/>
              <apply>
                <divide/>
                <ci> kfR </ci>
                <ci> Kact </ci>
              </apply>
              <ci> R_star </ci>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> kf </ci>
              <ci> L </ci>
              <ci> R </ci>
            </apply>
            <apply>
              <times/>
              <ci> kfR </ci>
              <ci> R </ci>
            </apply>
          </apply>
        </apply> 
      </apply> 
    </math>  
