  <!--
    Below, we define some additional units for association with variables and
    constants within the model. The identifiers are fairly self-explanatory.
  -->
  
  <units name="micromolar">
    <unit prefix="micro" units="mole"/>
    <unit exponent="-1" units="litre"/>
  </units>
  
  <units name="micrometre">
    <unit prefix="micro" units="metre"/>
  </units>
  
  <units name="micromolar_micrometre_per_second">
    <unit units="micromolar"/>
    <unit units="micrometre"/>
    <unit exponent="-1" units="second"/>
  </units>
  
  <units name="micrometre_per_second">
    <unit units="micrometre"/>
    <unit exponent="-1" units="second"/>
  </units>
  
  <units name="micrometre_2_per_second">
    <unit exponent="2" units="micrometre"/>
    <unit exponent="-1" units="second"/>
  </units>
  
  <units name="flux">
    <unit units="micromolar"/>
    <unit exponent="-1" units="second"/>
  </units>
  
  <units name="second_order_rate_constant">
    <unit exponent="-1" units="micromolar"/>
    <unit exponent="-1" units="second"/>
  </units>
  
  <!--
    The "environment" component is used to declare variables that are used by
    all or most of the other components, in this case just "time".
  -->
  
  <component name="environment">
    <variable name="time" public_interface="out" units="second"/>
  </component>
  
  <component cmeta:id="id_00013" name="Calcium_dynamics">
    <variable cmeta:id="id_00001" initial_value="0.05" name="Ca" public_interface="out" units="micromolar"/>
    
    <variable name="time" public_interface="in" units="second"/>
    <variable cmeta:id="id_00015" name="J_channel" public_interface="in" units="flux"/>
    <variable cmeta:id="id_00003" name="J_pump" public_interface="in" units="flux"/>
    <variable cmeta:id="id_00014" name="J_leak" public_interface="in" units="flux"/>
    <variable name="beta" public_interface="in" units="dimensionless"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="Ca_diff_eq">
        <eq/>
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> Ca </ci> 
        </apply>
        <apply>
          <times/>
          <ci> beta </ci>
          <apply>
            <plus/>
            <apply>
              <minus/>
              <ci> J_channel </ci>
              <ci> J_pump </ci> 
            </apply>
            <ci> J_leak </ci>
          </apply>
        </apply>
      </apply>
    </math> 
  </component>
  
  <component cmeta:id="id_00010" name="ER">
    <variable cmeta:id="id_00002" initial_value="400.0" name="Ca_ER" public_interface="out" units="micromolar"/>
  </component>
  
  <component name="Channel_kinetics">
    <variable cmeta:id="id_00011" name="J_channel" public_interface="out" units="flux"/>
    
    <variable cmeta:id="id_00012" initial_value="2880.0" name="J_max" units="flux"/>
    <variable initial_value="0.667" name="h" units="dimensionless"/>
    <variable initial_value="0.17" name="K_act" units="micromolar"/>
    <variable initial_value="0.03" name="K_I" units="micromolar"/>
    <variable initial_value="0.1" name="K_inh" units="micromolar"/>
    <variable initial_value="8.0" name="k_on" units="second_order_rate_constant"/>
    <variable initial_value="0.01" name="I" units="micromolar"/>
    
    <variable name="time" public_interface="in" units="second"/>
    <variable name="Ca" public_interface="in" units="micromolar"/>
    <variable name="Ca_ER" public_interface="in" units="micromolar"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_channel_calculation">
        <eq/>
        <ci> J_channel </ci>
        <apply>
          <times/>
          <ci> J_max </ci>
          <apply>
            <power/>
            <apply>
              <times/>
              <apply>
                <divide/>
                <ci> I </ci>
                <apply>
                  <plus/>
                  <ci> I </ci>
                  <ci> K_I </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Ca </ci>
                <apply>
                  <plus/>
                  <ci> Ca </ci>
                  <ci> K_act </ci>
                </apply>
              </apply>
              <ci> h </ci>
            </apply>
            <cn cellml:units="dimensionless"> 3.0 </cn>
          </apply>
          <apply>
            <minus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <divide/>
              <ci> Ca </ci>
              <ci> Ca_ER </ci>
            </apply>  
          </apply>
        </apply>
      </apply>
      
      <apply id="h_diff_eq">
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> h </ci>
        </apply>
        <apply>
          <times/>
          <ci> k_on </ci>
          <apply>
            <minus/>
            <ci> K_inh </ci>
            <apply>
              <times/>
              <ci> h </ci>
              <apply>
                <plus/>
                <ci> Ca </ci>
                <ci> K_inh </ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math> 
  </component>
  
  <component cmeta:id="id_00004" name="SERCA_pump_kinetics">
    <variable cmeta:id="id_00005" name="J_pump" public_interface="out" units="flux"/>
    
    <variable initial_value="5.85" name="V_max" units="flux"/>
    <variable initial_value="0.24" name="K_p" units="micromolar"/>
    
    <variable name="time" public_interface="in" units="second"/>
    <variable name="Ca" public_interface="in" units="micromolar"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_pump_calculation">
        <eq/>
        <ci> J_pump </ci>
        <apply>
          <times/>
          <ci> V_max </ci>
          <apply>
            <divide/>
            <apply>
              <power/>
              <ci> Ca </ci>
              <cn cellml:units="dimensionless"> 2.0 </cn>
            </apply>
            <apply>
              <plus/>
              <apply>
                <power/>
                <ci> Ca </ci>
                <cn cellml:units="dimensionless"> 2.0 </cn>
              </apply>
              <apply>
                <power/>
                <ci> K_p </ci>
                <cn cellml:units="dimensionless"> 2.0 </cn>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math> 
  </component>
  
  <component name="Calcium_buffering">
    <variable name="beta" public_interface="out" units="dimensionless"/>
    
    <variable initial_value="12.0" name="B_end" units="micromolar"/>
    <variable initial_value="11.35" name="B_ex" units="micromolar"/>
    <variable initial_value="0.3" name="K_end" units="micromolar"/>
    <variable initial_value="0.26" name="K_ex" units="micromolar"/>
    <variable name="Ca_ex" units="micromolar"/>
    
    <variable name="time" public_interface="in" units="second"/>
     
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="beta_calculation">
        <eq/>
        <ci> beta </ci>
        <apply>
          <power/>
          <apply>
            <plus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <divide/>
              <ci> B_end </ci>
              <ci> K_end </ci>
            </apply>
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> B_ex </ci>
                <ci> K_ex </ci>
              </apply>
              <apply>
                <power/>
                <apply>
                  <plus/>
                  <ci> Ca_ex </ci>
                  <ci> K_ex </ci>
                </apply>
                <cn cellml:units="dimensionless"> 2.0 </cn>
              </apply>
            </apply>
          </apply>
          <cn cellml:units="dimensionless"> -1.0 </cn>
        </apply>
      </apply>
    </math> 
  </component>
  
  <component cmeta:id="id_00006" name="Leak">
    <variable cmeta:id="id_00007" name="J_leak" public_interface="out" units="flux"/>
    
    <variable initial_value="0.0804" name="L" units="flux"/>
    
    <variable name="time" public_interface="in" units="second"/>
    <variable cmeta:id="id_00008" name="Ca" public_interface="in" units="micromolar"/>
    <variable cmeta:id="id_00009" name="Ca_ER" public_interface="in" units="micromolar"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="J_leak_calculation">
        <eq/>
        <ci> J_leak </ci>
        <apply>
          <times/>
          <ci> L </ci>
          <apply>
            <minus/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <divide/>
              <ci> Ca </ci>
              <ci> Ca_ER </ci>
            </apply>
          </apply>
        </apply>
      </apply>
    </math> 
  </component>
  
  <connection>
    <map_components component_1="Calcium_dynamics" component_2="environment"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
  <connection>
    <map_components component_1="Channel_kinetics" component_2="environment"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
  <connection>
    <map_components component_1="SERCA_pump_kinetics" component_2="environment"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
  <connection>
    <map_components component_1="Leak" component_2="environment"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
  <connection>
    <map_components component_1="Calcium_buffering" component_2="environment"/>
    <map_variables variable_1="time" variable_2="time"/>
  </connection>
  
  <connection>
    <map_components component_1="Calcium_dynamics" component_2="Channel_kinetics"/>
    <map_variables variable_1="Ca" variable_2="Ca"/>
    <map_variables variable_1="J_channel" variable_2="J_channel"/>
  </connection>
  
  <connection>
    <map_components component_1="Calcium_dynamics" component_2="SERCA_pump_kinetics"/>
    <map_variables variable_1="Ca" variable_2="Ca"/>
    <map_variables variable_1="J_pump" variable_2="J_pump"/>
  </connection>
  
  <connection>
    <map_components component_1="Calcium_dynamics" component_2="Leak"/>
    <map_variables variable_1="Ca" variable_2="Ca"/>
    <map_variables variable_1="J_leak" variable_2="J_leak"/>
  </connection>
  
  <connection>
    <map_components component_1="Calcium_dynamics" component_2="Calcium_buffering"/>
    <map_variables variable_1="beta" variable_2="beta"/>
  </connection>
  
  <connection>
    <map_components component_1="ER" component_2="Channel_kinetics"/>
    <map_variables variable_1="Ca_ER" variable_2="Ca_ER"/>
  </connection>
  
  <connection>
    <map_components component_1="ER" component_2="Leak"/>
    <map_variables variable_1="Ca_ER" variable_2="Ca_ER"/>
  </connection>



