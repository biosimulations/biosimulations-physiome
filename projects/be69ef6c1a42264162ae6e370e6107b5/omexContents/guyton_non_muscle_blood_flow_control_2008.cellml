						<apply>
							<lt/>
							<ci>AR3T</ci>
							<cn cellml:units="dimensionless">0.3</cn>
						</apply>
					</piece>
					<otherwise>
						<ci>AR3T</ci>
					</otherwise>
				</piecewise>
			</apply>
		</math>
	</component>

