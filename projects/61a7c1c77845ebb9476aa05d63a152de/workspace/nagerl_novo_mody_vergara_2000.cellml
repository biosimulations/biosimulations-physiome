   <units name="micromolar">
      <unit units="mole" prefix="micro"/>
      <unit units="litre" exponent="-1"/>
   </units>
   
   <units name="millimolar">
      <unit units="mole" prefix="milli"/>
      <unit units="litre" exponent="-1"/>
   </units>
   
   <units name="millisecond">
      <unit units="second" prefix="milli"/>
   </units>

   <units name="microsecond">
      <unit units="second" prefix="micro"/>
   </units>
   
   <units name="flux">
      <unit units="micromolar" exponent="1"/>
      <unit units="millisecond" exponent="-1"/>
   </units>
   
   <units name="first_order_rate_constant">
      <unit units="millisecond" exponent="-1"/>
   </units>
  
   <units name="second_order_rate_constant">
      <unit units="micromolar" exponent="-1"/>
      <unit units="millisecond" exponent="-1"/>
   </units>
  
   
   <component name="environment">
      <variable units="millisecond" public_interface="out" name="time"/>
   </component>
   
   
   <component cmeta:id="Ca" name="Ca">
      <variable units="micromolar" public_interface="out" name="Ca" initial_value="1.5"/>
     
      <variable units="micromolar" public_interface="in" name="CaD"/>
      <variable units="micromolar" public_interface="in" name="CaDM1"/>
      <variable units="micromolar" public_interface="in" name="CaDM2"/>
      <variable units="micromolar" public_interface="in" name="CaBi"/>
      
      <variable units="micromolar" name="Ca_T"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="1">
         <apply>
        <eq/>
            <ci>Ca</ci>
            <apply>
          <minus/>
              <ci>Ca_T</ci>
              <apply>
            <plus/>
                 <ci>CaD</ci>
                 <ci>CaDM1</ci>
                 <ci>CaDM2</ci>
                 <ci>CaBi</ci>
              </apply>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="DM_T" name="DM_T">
      <variable units="millimolar" public_interface="out" name="DM_T" initial_value="5.0"/>
   </component>

   <component cmeta:id="DM1" name="DM1">
      <variable units="millimolar" public_interface="out" name="DM1"/>
      
      <variable units="dimensionless" public_interface="in" name="alpha"/>
      <variable units="millimolar" public_interface="in" name="DM_T"/>
      <variable units="micromolar" public_interface="in" name="CaDM1"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="3">
         <apply>
        <eq/>
            <ci>DM1</ci>
            <apply>
          <minus/>
              <apply>
            <times/>
                <ci>DM_T</ci>
                <ci>alpha</ci>
              </apply>
              <ci>CaDM1</ci>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="DM1_T" name="DM1_T">
      <variable units="millimolar" public_interface="out" name="DM1_T"/>
      
      <variable units="millimolar" public_interface="in" name="DM1"/>
      <variable units="micromolar" public_interface="in" name="CaDM1"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="4">
         <apply>
        <eq/>
            <ci>DM1_T</ci>
            <apply>
          <plus/>
              <ci>DM1</ci>
              <ci>CaDM1</ci>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="DM2" name="DM2">
      <variable units="micromolar" public_interface="out" name="DM2"/>
     
      <variable units="dimensionless" public_interface="in" name="alpha"/>
      <variable units="millimolar" public_interface="in" name="DM_T"/>
      <variable units="micromolar" public_interface="in" name="CaDM2"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="5">
         <apply>
        <eq/>
            <ci>DM2</ci>
            <apply>
          <minus/>
              <apply>
            <times/>
                <ci>DM_T</ci>
                <apply>
              <minus/>
                  <cn cellml:units="dimensionless"> 1.0 </cn>
                  <ci>alpha</ci>
                </apply>
              </apply>
              <ci>CaDM2</ci>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="DM2_T" name="DM2_T">
      <variable units="millimolar" public_interface="out" name="DM2_T"/>
      
      <variable units="millimolar" public_interface="in" name="DM2"/>
      <variable units="micromolar" public_interface="in" name="CaDM2"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="6">
         <apply>
        <eq/>
            <ci>DM2_T</ci>
            <apply>
          <plus/>
              <ci>DM2</ci>
              <ci>CaDM2</ci>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="D" name="D">
      <variable units="micromolar" name="D_T" initial_value="100.0"/>
     
      <variable units="micromolar" public_interface="out" name="D"/>
      <variable units="micromolar" public_interface="in" name="CaD"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="7">
         <apply>
        <eq/>
            <ci>D</ci>
            <apply>
          <minus/>
               <ci>D_T</ci>
               <ci>CaD</ci>
            </apply>
         </apply>
      </math>
   </component>

   <component cmeta:id="Bi" name="Bi">
      <variable units="micromolar" name="Bi_T" initial_value="1.0"/>
     
      <variable units="micromolar" public_interface="out" name="Bi"/>
      <variable units="micromolar" public_interface="in" name="CaBi"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="8">
         <apply>
        <eq/>
            <ci>Bi_T</ci>
            <apply>
          <plus/>
               <ci>Bi</ci>
               <ci>CaBi</ci>
            </apply>
         </apply>
      </math>
   </component>
   
   <component cmeta:id="CaDM1" name="CaDM1">
      <variable units="micromolar" public_interface="out" name="CaDM1"/>
      
      <variable units="micromolar" public_interface="in" name="Ca"/>
      <variable units="micromolar" public_interface="in" name="DM1"/>
      <variable units="second_order_rate_constant" public_interface="in" name="kon_DM"/>
      <variable units="first_order_rate_constant" public_interface="in" name="koff_DM"/>
      <variable units="first_order_rate_constant" public_interface="in" name="k_trans"/>
      <variable units="millisecond" public_interface="in" name="t_pulse"/>
      <variable units="millisecond" public_interface="in" name="time"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="9">
        <apply>
        <eq/>
          <apply>
          <diff/>
            <bvar>
            <ci> time </ci>
          </bvar>
            <ci> CaDM1 </ci>
          </apply>
          <piecewise>
            <piece>
              <apply>
              <minus/>
                <apply>
                <times/>
                  <ci> kon_DM </ci>
                  <ci> Ca </ci>
                  <ci> DM1 </ci>
                </apply>
                <apply>
                <times/>
                  <ci> koff_DM </ci>
                  <ci> CaDM1 </ci>
                </apply>
              </apply>
              <apply>
              <eq/>
                <ci> t_pulse </ci>
                <cn cellml:units="dimensionless"> 0.0 </cn>
              </apply>
            </piece>
            <otherwise>
              <apply>
              <minus/>
                <apply>
                <times/>
                  <ci> kon_DM </ci>
                  <ci> Ca </ci>
                  <ci> DM1 </ci>
                </apply>
                <apply>
                <times/>
                  <ci> k_trans </ci>
                  <ci> CaDM1 </ci>
                </apply>
              </apply>
            </otherwise>
          </piecewise>
        </apply>
      </math>
   </component>

   <component cmeta:id="CaDM2" name="CaDM2">
      <variable units="micromolar" public_interface="out" name="CaDM2"/>
      
      <variable units="micromolar" public_interface="in" name="Ca"/>
      <variable units="micromolar" public_interface="in" name="DM2"/>
      <variable units="second_order_rate_constant" public_interface="in" name="kon_DM"/>
      <variable units="first_order_rate_constant" public_interface="in" name="koff_DM"/>
      <variable units="millisecond" public_interface="in" name="time"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="10">
        <apply>
        <eq/>
          <apply>
          <diff/>
            <bvar>
            <ci> time </ci>
          </bvar>
            <ci> CaDM2 </ci>
          </apply>
          <apply>
          <minus/>
            <apply>
            <times/>
              <ci> kon_DM </ci>
              <ci> Ca </ci>
              <ci> DM2 </ci>
            </apply>
            <apply>
            <times/>
              <ci> koff_DM </ci>
              <ci> CaDM2 </ci>
            </apply>
          </apply>
        </apply>
      </math>
   </component>

   <component cmeta:id="CaD" name="CaD">
      <variable units="micromolar" public_interface="out" name="CaD"/>
      
      <variable units="micromolar" public_interface="in" name="Ca"/>
      <variable units="micromolar" public_interface="in" name="D"/>
      <variable units="second_order_rate_constant" public_interface="in" name="kon_D"/>
      <variable units="first_order_rate_constant" public_interface="in" name="koff_D"/>
      <variable units="millisecond" public_interface="in" name="time"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="11">
        <apply>
        <eq/>
          <apply>
          <diff/>
            <bvar>
            <ci> time </ci>
          </bvar>
            <ci> CaD </ci>
          </apply>
          <apply>
          <minus/>
            <apply>
            <times/>
              <ci> kon_D </ci>
              <ci> Ca </ci>
              <ci> D </ci>
            </apply>
            <apply>
            <times/>
              <ci> koff_D </ci>
              <ci> CaD </ci>
            </apply>
          </apply>
        </apply>
      </math>
   </component>

   <component cmeta:id="CaBi" name="CaBi">
      <variable units="micromolar" public_interface="out" name="CaBi"/>
      
      <variable units="micromolar" public_interface="in" name="Ca"/>
      <variable units="micromolar" public_interface="in" name="Bi"/>
      <variable units="second_order_rate_constant" public_interface="in" name="kon_Bi"/>
      <variable units="first_order_rate_constant" public_interface="in" name="koff_Bi"/>
      <variable units="millisecond" public_interface="in" name="time"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML" id="12">
        <apply>
        <eq/>
          <apply>
          <diff/>
            <bvar>
            <ci> time </ci>
          </bvar>
            <ci> CaBi </ci>
          </apply>
          <apply>
          <minus/>
            <apply>
            <times/>
              <ci> kon_Bi </ci>
              <ci> Ca </ci>
              <ci> Bi </ci>
            </apply>
            <apply>
            <times/>
              <ci> koff_Bi </ci>
              <ci> CaBi </ci>
            </apply>
          </apply>
        </apply>
      </math>
   </component>
   
   <component name="constants">
     <variable units="second_order_rate_constant" public_interface="out" name="kon_DM" initial_value="0.03"/>
     <variable units="first_order_rate_constant" public_interface="out" name="koff_DM" initial_value="0.00006"/>
     <variable units="second_order_rate_constant" public_interface="out" name="kon_D" initial_value="0.124"/>
     <variable units="first_order_rate_constant" public_interface="out" name="koff_D" initial_value="5.6"/>
     <variable units="second_order_rate_constant" public_interface="out" name="kon_Bi" initial_value="1.0"/>
     <variable units="first_order_rate_constant" public_interface="out" name="koff_Bi" initial_value="1.0"/>
     <variable units="first_order_rate_constant" public_interface="out" name="k_trans"/>
     <variable units="millisecond" public_interface="out" name="t_pulse" initial_value="1.0"/>
     <variable units="dimensionless" public_interface="out" name="alpha" initial_value="0.5"/>
     
     <variable units="first_order_rate_constant" name="koff_DM_" initial_value="75.0"/>
     <variable units="microsecond" name="tau_photolysis" initial_value="20.0"/>
     
     <variable units="millisecond" public_interface="in" name="time"/>

     <math xmlns="http://www.w3.org/1998/Math/MathML" id="13">
        <apply>
        <eq/>
          <ci> k_trans </ci>
          <apply>
          <plus/>
            <ci> koff_DM </ci>
            <apply>
            <times/>
              <apply>
              <minus/>
                <ci> koff_DM_ </ci>
                <ci> koff_DM </ci>
              </apply>
              <apply>
              <minus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
                <apply>
                <exp/>
                  <apply>
                  <divide/>
                    <apply>
                    <minus/>
                      <apply>
                      <minus/>
                        <ci> time </ci>
                        <ci> t_pulse </ci>
                      </apply>
                    </apply>
                    <ci> tau_photolysis </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </math>
    </component>   
  `
  
