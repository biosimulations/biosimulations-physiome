                    <apply>
                      <divide/>
                      <ci> Vm </ci>
                      <cn cellml:units="millivolt"> 2.5 </cn>
                    </apply>
                  </apply>
                  <cn cellml:units="dimensionless"> 1.0 </cn>
                </apply>
              </apply>
            </apply>
          </otherwise>
        </piecewise>
      </apply>
    
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> m </ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> alpha_m </ci>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <ci> m </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> beta_m </ci>
            <ci> m </ci>
          </apply>
        </apply>
      </apply>
    </math>
