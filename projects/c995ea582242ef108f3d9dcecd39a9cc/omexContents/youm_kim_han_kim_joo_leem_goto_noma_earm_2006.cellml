  <component cmeta:id="I_NaCa_y_gate" name="I_NaCa_y_gate">
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
      <rdf:Description rdf:about="I_NaCa_y_gate">
        <dc:title>I_NaCa_y_gate</dc:title>
        <dcterms:alternative>sodium calcium exchange current y gate</dcterms:alternative>  
      </rdf:Description>
    </rdf:RDF>
    <variable units="dimensionless" public_interface="out" name="y" initial_value="0.9459136"/> 
    <variable units="dimensionless" public_interface="out" name="m"/>
    <variable units="dimensionless" public_interface="out" name="h"/>
    
    <variable units="first_order_rate_constant" name="alpha_y"/>
    <variable units="first_order_rate_constant" name="beta_y"/>
    
    <variable units="kelvin" public_interface="in" name="T"/>
    <variable units="coulomb_millivolt_kelvin_millimole" public_interface="in" name="R"/>
    <variable units="coulomb_millimole" public_interface="in" name="F"/>
    <variable units="millivolt" public_interface="in" name="Vm"/>
    <variable units="millisecond" public_interface="in" name="time"/>
    <variable units="dimensionless" public_interface="in" name="p_E1Na"/>
    <variable units="dimensionless" public_interface="in" name="p_E2Na"/>
    <variable units="dimensionless" public_interface="in" name="p_E1Ca"/>
    <variable units="dimensionless" public_interface="in" name="p_E2Ca"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="13">
      <apply>
        <eq/>
        <ci> alpha_y </ci>
        <apply>
          <times/>
          <cn cellml:units="first_order_rate_constant"> 1.0 </cn>
          <apply>
            <plus/>
            <ci> h </ci>
            <ci> p_E2Ca </ci>
          </apply>
        </apply>
      </apply>
    
      <apply>
        <eq/>
        <ci> beta_y </ci>
        <apply>
          <times/>
          <cn cellml:units="first_order_rate_constant"> 1.0 </cn>
          <apply>
            <plus/>
            <ci> m </ci>
            <ci> p_E1Ca </ci>
          </apply>
        </apply>
      </apply>

      <apply>
        <eq/>
        <ci> m </ci>
	<apply>
          <times/>
          <ci> p_E1Na </ci>
          <apply>
            <exp/>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> 0.32 </cn>
              <apply>
                <divide/>
                <ci> Vm </ci>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> R </ci>
                    <ci> T </ci>
                  </apply>
                  <ci> F </ci>
                </apply>
              </apply>
	    </apply>
          </apply>
        </apply>
      </apply>

      <apply>
        <eq/>
        <ci> h </ci>
	<apply>
          <times/>
          <ci> p_E2Na </ci>
          <apply>
            <exp/>
            <apply>
              <times/>
              <apply>
                <minus/>
                <cn cellml:units="dimensionless"> 0.32 </cn>
                <cn cellml:units="dimensionless"> 1.0 </cn>
              </apply>
              <apply>
                <divide/>
                <ci> Vm </ci>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> R </ci>
                    <ci> T </ci>
                  </apply>
                  <ci> F </ci>
                </apply>
              </apply>
	    </apply>
          </apply>
        </apply>
      </apply>
    
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci> time </ci>
          </bvar>
          <ci> y </ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> alpha_y </ci>
            <apply>
              <minus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <ci> y </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> beta_y </ci>
            <ci> y </ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

