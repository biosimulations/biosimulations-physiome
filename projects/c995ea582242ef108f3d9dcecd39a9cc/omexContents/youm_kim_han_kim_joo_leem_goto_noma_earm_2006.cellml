    <variable units="picoampere" public_interface="out" name="I_tr"/>
    
    <variable units="picoampere_per_millimolar" name="ptr" initial_value="20.5"/>
       
    <variable units="millimolar" public_interface="in" name="Ca_up"/>
    <variable units="millimolar" public_interface="in" name="Ca_rel"/>
     
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="18">
      <apply>
        <eq/>
        <ci> I_tr </ci>
        <apply>
          <times/>
          <cn cellml:units="dimensionless"> 2.8 </cn>
          <ci> ptr </ci>
          <apply>
            <minus/>
            <ci> Ca_up </ci>
            <ci> Ca_rel </ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

