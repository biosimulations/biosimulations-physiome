    <variable units="millisecond" public_interface="in" name="time"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="6">
      <apply>
        <eq/>
        <ci> alpha_m </ci>
        <piecewise>
          <piece>
            <apply>
              <times/>
              <cn cellml:units="per_millivolt_millisecond"> 0.002175 </cn>
              <apply>
                <divide/>
                <apply>
                  <plus/>
                  <ci> Vm </ci>
                  <cn cellml:units="millivolt"> 30.0 </cn>
                </apply>
                <apply>
                  <minus/>
                  <cn cellml:units="dimensionless"> 1.0 </cn>
                  <apply>
                    <exp/>
                    <apply>
                      <divide/>
                      <apply>
                        <plus/>
                        <ci> Vm </ci>
                        <cn cellml:units="millivolt"> 30.0 </cn>
                      </apply>
                      <apply>
                        <minus/>
                        <cn cellml:units="millivolt"> 2.5 </cn>
                      </apply>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <eq/>
              <ci> Vm </ci>
              <cn cellml:units="millivolt"> -30.0 </cn>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="first_order_rate_constant"> 0.004189268 </cn>
          </otherwise>
        </piecewise>
      </apply>
    
      <apply>
        <eq/>
        <ci> beta_m </ci>
        <piecewise>
          <piece>
            <cn cellml:units="first_order_rate_constant"> 0.001578959 </cn>
            <apply>
              <eq/>
              <ci> Vm </ci>
              <cn cellml:units="millivolt"> 0.0 </cn>
            </apply>
          </piece>
          <otherwise>
            <apply>
              <times/>
              <cn cellml:units="per_millivolt_millisecond"> 0.0006315 </cn>
              <apply>
                <divide/>
                <ci> Vm </ci>
                <apply>
