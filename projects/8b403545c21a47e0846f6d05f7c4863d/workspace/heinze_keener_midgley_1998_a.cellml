    <variable name="kfb" public_interface="in" units="second_order_rate_constant"/>
    <variable name="kdf" public_interface="in" units="first_order_rate_constant"/>
    <variable name="D" public_interface="in" units="dimensionless"/>
    <variable name="GnRH" public_interface="in" units="nanomolar"/>
    <variable name="time" public_interface="in" units="hour"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci> F </ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> kdf </ci>
            <ci> D </ci>
          </apply>
          <apply>
            <times/>
            <ci> kfb </ci>
            <ci> F </ci>
            <ci> GnRH </ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

