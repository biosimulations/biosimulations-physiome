  <import xlink:href="../../common/units.xml">
    <units name="ms" units_ref="ms"/>
    <units name="mscu" units_ref="mscu"/>
    <units name="per_ms" units_ref="per_ms"/>
    <units name="mV" units_ref="mV"/>
    <units name="per_mV" units_ref="per_mV"/>
    <units name="nS_per_pF" units_ref="nS_per_pF"/>
    <units name="pA_per_pF" units_ref="pA_per_pF"/>
    <units name="mM" units_ref="mM"/>
  </import>
  
  <component name="IK1" cmeta:id="IK1">
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#IKs">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            Here we calculate the inward rectifier current. We expect 
            the time, membrane potential, reversal potential and conductance 
            to be set externally, and we provide access to the gating 
            variables and the calculated current. This component encapsulates
            all others in this model.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>
    
    <variable name="IK1" public_interface="out" units="pA_per_pF"/>         

    <variable name="g_K1" public_interface="in" units="nS_per_pF"/>
    <variable name="V" public_interface="in" private_interface="out" units="mV"/>  
    <variable name="Ko" public_interface="in" units="mM"/>
    <variable name="E_K" public_interface="in" private_interface="out" units="mV"/>           
    
    <variable name="K1_infinity" private_interface="in" units="dimensionless"/>
      
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="i_K1_calculation"><eq/>
        <ci>IK1</ci>
        <apply><times/>
          <ci>g_K1</ci>
          <apply><root/>
            <apply><divide/>
              <ci>Ko</ci>
              <cn cellml:units="mM">5.4</cn>
            </apply>
          </apply>
          <ci>K1_infinity</ci>
          <apply><minus/>
            <ci>V</ci>
            <ci>E_K</ci>
          </apply>
        </apply>
      </apply> 
    </math>
  </component>
  
  <component name="K1_gate" cmeta:id="K1_gate">
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#K1_gate">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            The activation gate kinetics.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>

    <variable name="K1_infinity" public_interface="out" units="dimensionless"/>
   
    <variable name="alpha_K1" units="dimensionless"/>
    <variable name="beta_K1" units="dimensionless"/>
    
    <variable name="E_K" public_interface="in" units="mV"/>
    <variable name="V" public_interface="in" units="mV"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="K1_infinity_calculation"><eq/>
        <ci>K1_infinity</ci>
        <apply><divide/>
          <ci>alpha_K1</ci>
          <apply><plus/>
            <ci>alpha_K1</ci>
            <ci>beta_K1</ci>
          </apply>
        </apply>
      </apply>
      
      <apply id="alpha_K1_calculation"><eq/>
        <ci>alpha_K1</ci>
        <apply><divide/>
          <cn cellml:units="dimensionless">0.1</cn>
          <apply><plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply><exp/>
              <apply><times/>
                <cn cellml:units="per_mV">0.06</cn>
                <apply><minus/>
                  <ci>V</ci>
                  <apply><plus/>
                    <ci>E_K</ci>
                    <cn cellml:units="mV">200.0</cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="beta_K1_calculation"><eq/>
        <ci>beta_K1</ci>
        <apply><divide/>
          <apply><plus/>
            <apply><times/>
              <cn cellml:units="dimensionless">3.0</cn>
              <apply><exp/>
                <apply><times/>
                  <cn cellml:units="per_mV">0.0002</cn>
                  <apply><plus/>
                    <apply><minus/>
                      <ci>V</ci>
                      <ci>E_K</ci>
                    </apply>
                    <cn cellml:units="mV">100.0</cn>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply><exp/>
              <apply><times/>
                <cn cellml:units="per_mV">0.1</cn>
                <apply><minus/>
                  <ci>V</ci>
                  <apply><plus/>
                    <ci>E_K</ci>
                    <cn cellml:units="mV">10.0</cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply><plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply><exp/>
              <apply><times/>
                <cn cellml:units="per_mV">-0.5</cn>
                <apply><minus/>
                  <ci>V</ci>
                  <ci>E_K</ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

  <group>
    <relationship_ref relationship="encapsulation"/>
    <component_ref component="IK1">
      <component_ref component="K1_gate"/>
    </component_ref>
  </group>

  <connection>
    <map_components component_1="IK1" component_2="K1_gate"/>
    <map_variables variable_1="K1_infinity" variable_2="K1_infinity"/>
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="E_K" variable_2="E_K"/>
  </connection>

</model>
