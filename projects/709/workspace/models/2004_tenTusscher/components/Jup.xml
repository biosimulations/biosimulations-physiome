  <import xlink:href="../../common/units.xml">
    <units name="mM" units_ref="mM"/>
    <units name="mM_per_ms" units_ref="mM_per_ms"/>
  </import>
  
  <component name="Jup" cmeta:id="Jup">
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#Jup">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            Calculate the calcium uptake flux from the SR.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>
    
    <variable name="Jup" public_interface="out" units="mM_per_ms"/>

    <variable name="Vmax_up" public_interface="in" units="mM_per_ms"/>
    <variable name="Cai" public_interface="in" units="mM"/>  
    <variable name="K_up" public_interface="in" units="mM"/>        
    
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="Jup_calculation"><eq/>
        <ci>Jup</ci>
        <apply><divide/>
          <ci>Vmax_up</ci>
          <apply><plus/>
            <cn cellml:units="dimensionless">1.0</cn>
            <apply><divide/>
              <apply><power/>
                <ci>K_up</ci>
                <cn cellml:units="dimensionless">2.0</cn>
              </apply>
              <apply><power/>
                <ci>Cai</ci>
                <cn cellml:units="dimensionless">2.0</cn>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply> 
    </math>
  </component>
  
