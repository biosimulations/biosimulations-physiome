    <units name="mM_per_ms" units_ref="mM_per_ms"/>
    <units name="per_mm" units_ref="per_mm"/>
    <units name="uF_per_cmsq" units_ref="uF_per_cmsq"/>
    <units name="umcu" units_ref="umcu"/>
    <units name="nS_per_pF" units_ref="nS_per_pF"/>
    <units name="K" units_ref="K"/>
    <units name="cmcu_per_uF_per_s" units_ref="cmcu_per_uF_per_s"/>
    <units name="per_ms" units_ref="per_ms"/>
    <units name="mM_per_s" units_ref="mM_per_s"/>
    <units name="faradays_constant" units_ref="faradays_constant"/>
  </import>
  
  <component name="sodium_dynamics" cmeta:id="sodium_dynamics">
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#sodium_dynamics">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            The intracellular sodium dynamics.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>

    <variable name="Nai" initial_value="Nai_initial" public_interface="out" units="mM"/>
    
    <variable name="Nai_initial" public_interface="in" units="mM"/>
    <variable name="vC" public_interface="in" units="umcu"/>
    <variable name="Am" public_interface="in" units="per_mm"/>
    <variable name="F" public_interface="in" units="faradays_constant"/>
    <variable name="INa" public_interface="in" units="pA_per_pF"/>
    <variable name="IbNa" public_interface="in" units="pA_per_pF"/>
    <variable name="INaK" public_interface="in" units="pA_per_pF"/>
    <variable name="INaCa" public_interface="in" units="pA_per_pF"/>
    <variable name="time" public_interface="in" units="ms"/>
    <variable name="Capacitance" public_interface="in" units="uF_per_cmsq"/>

    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply id="Nai_total_diff"><eq />
        <apply><diff/>
          <bvar><ci>time</ci></bvar>
          <ci> Nai </ci>
        </apply>
        <apply><minus/>
          <apply><times/>
            <ci>Capacitance</ci>
            <apply><divide />
              <apply><plus />
                <ci> INa </ci>
                <ci> IbNa </ci>
                <apply><times />
                  <ci> INaK </ci>
                  <cn cellml:units="dimensionless"> 3.0 </cn>
                </apply>
                <apply><times />
                  <ci> INaCa </ci>
                  <cn cellml:units="dimensionless"> 3.0 </cn>
                </apply>
              </apply>
              <apply><times />
                <ci> vC </ci>
                <ci> F </ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
    
</model>
