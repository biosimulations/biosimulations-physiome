  <import xlink:href="../../common/units.xml">
    <units name="mM" units_ref="mM"/>
    <units name="mM_per_ms" units_ref="mM_per_ms"/>
    <units name="ms" units_ref="ms"/>
    <units name="mV" units_ref="mV"/>
    <units name="umcu" units_ref="umcu"/>
    <units name="per_mm" units_ref="per_mm"/>
    <units name="pA_per_pF" units_ref="pA_per_pF"/>
    <units name="uF_per_cmsq" units_ref="uF_per_cmsq"/>
    <units name="faradays_constant" units_ref="faradays_constant"/>
  </import>
  
  <component name="calcium_dynamics" cmeta:id="calcium_dynamics">
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#calcium_dynamics">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            The intracellular calcium dynamics.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>

    <!--
    <variable name="Cai_dummy" initial_value="0.2e-3" public_interface="out" private_interface="out" units="mM"/>
    <variable name="CaSR_dummy" initial_value="0.2" public_interface="out" private_interface="out" units="mM"/>
    -->
    
    <variable name="Cai" public_interface="out" units="mM"/>
    <variable name="CaSR" public_interface="out" units="mM"/>
    <variable name="Cai_total" public_interface="out" initial_value="Cai_total_initial" units="mM"/>
    <variable name="CaSR_total" public_interface="out" initial_value="CaSR_total_initial" units="mM"/>
    <variable name="Cai_buf" public_interface="out" units="mM"/>
    <variable name="CaSR_buf" public_interface="out" units="mM"/>
    
    <variable name="Cai_total_initial" public_interface="in" units="mM"/>
    <variable name="CaSR_total_initial" public_interface="in" units="mM"/>
    <variable name="Bufc" public_interface="in" units="mM"/>
    <variable name="Kbufc" public_interface="in" units="mM"/>
    <variable name="Bufsr" public_interface="in" units="mM"/>
    <variable name="Kbufsr" public_interface="in" units="mM"/>
    <variable name="vSR" public_interface="in" units="umcu"/>
    <variable name="vC" public_interface="in" units="umcu"/>
    <variable name="Am" public_interface="in" units="per_mm"/>
    <variable name="F" public_interface="in" units="faradays_constant"/>
    <variable name="ICaL" public_interface="in" units="pA_per_pF"/>
    <variable name="IbCa" public_interface="in" units="pA_per_pF"/>
    <variable name="IpCa" public_interface="in" units="pA_per_pF"/>
    <variable name="INaCa" public_interface="in" units="pA_per_pF"/>
    <variable name="Jleak" public_interface="in" units="mM_per_ms"/>
    <variable name="Jup" public_interface="in" units="mM_per_ms"/>
    <variable name="Jrel" public_interface="in" units="mM_per_ms"/>
    <variable name="time" public_interface="in" units="ms"/>
    <variable name="Capacitance" public_interface="in" units="uF_per_cmsq"/>
    
    <units name="mMsq">
      <unit units="mM" exponent="2"/>
    </units>
    
    <variable name="aCai" units="dimensionless"/>
    <variable name="bCai" units="mM"/>
    <variable name="cCai" units="mMsq"/>
    <variable name="aCaSR" units="dimensionless"/>
    <variable name="bCaSR" units="mM"/>
    <variable name="cCaSR" units="mMsq"/>
    
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#Cai">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            The differential equation defining the total intracellular
            calcium concentration - Cai_total = Cai + Cai_buf.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>
    <math xmlns="http://www.w3.org/1998/Math/MathML" cmeta:id="Cai">
      <apply><eq/>
        <ci>aCai</ci>
        <cn cellml:units="dimensionless">-1.0</cn>
      </apply>
      <apply><eq/>
        <ci>bCai</ci>
        <apply><plus/>
          <ci>Cai_total</ci>
          <ci>Kbufc</ci>
          <ci>Bufc</ci>
        </apply>
      </apply>
      <apply><eq/>
        <ci>cCai</ci>
        <apply><minus/>
          <apply><times/>
            <ci>Bufc</ci>
            <ci>Cai_total</ci>
          </apply>
        </apply>
      </apply>
      <apply><eq/>
        <ci>Cai_buf</ci>
        <apply><divide/>
          <apply><minus/>
            <apply><root/>
              <apply><minus/>
                <apply><power/>
                  <ci>bCai</ci>
                  <cn cellml:units="dimensionless">2.0</cn>
                </apply>
                <apply><times/>
                  <cn cellml:units="dimensionless">4.0</cn>
                  <ci>aCai</ci>
                  <ci>cCai</ci>
                </apply>
              </apply>
            </apply>
            <ci>bCai</ci>
          </apply>
          <apply><times/>
            <cn cellml:units="dimensionless">2.0</cn>
            <ci>aCai</ci>
          </apply>
        </apply>
      </apply>
      <apply><eq/>
        <ci>Cai</ci>
        <apply><minus/>
          <ci>Cai_total</ci>
          <ci>Cai_buf</ci>
        </apply>
      </apply>
      <apply id="Cai_total_diff"><eq />
        <apply><diff/>
          <bvar><ci>time</ci></bvar>
          <ci> Cai_total </ci>
        </apply>
        <apply><minus/>
          <apply><plus/>
            <ci>Jleak</ci>
            <ci>Jrel</ci>
          </apply>
          <apply><plus/>
            <ci>Jup</ci>
            <apply><times/>
              <ci>Capacitance</ci>
              <apply><divide/>
                <apply><minus/>
                  <apply><plus/>
                    <ci>ICaL</ci>
                    <ci>IbCa</ci>
                    <ci>IpCa</ci>
                  </apply>
                  <apply><times/>
                    <cn cellml:units="dimensionless">2.0</cn>
                    <ci>INaCa</ci>
                  </apply>
                </apply>
                <apply><times/>
                  <cn cellml:units="dimensionless">2.0</cn>
                  <ci>vC</ci>
                  <ci>F</ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
    <rdf:RDF
      xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
      xmlns:cmeta="http://www.cellml.org/metadata/1.0#">
      <rdf:Description rdf:about="#CaSR">
        <cmeta:comment rdf:parseType="Resource">
          <rdf:value>
            The differential equation defining the total SR
            calcium concentration - CaSR_total = CaSR + CaSR_buf.
          </rdf:value>
        </cmeta:comment>
      </rdf:Description>
    </rdf:RDF>
    <math xmlns="http://www.w3.org/1998/Math/MathML" cmeta:id="CaSR">
      <apply><eq/>
        <ci>aCaSR</ci>
        <cn cellml:units="dimensionless">-1.0</cn>
      </apply>
      <apply><eq/>
        <ci>bCaSR</ci>
        <apply><plus/>
          <ci>CaSR_total</ci>
          <ci>Kbufsr</ci>
          <ci>Bufsr</ci>
        </apply>
      </apply>
      <apply><eq/>
        <ci>cCaSR</ci>
        <apply><minus/>
          <apply><times/>
            <ci>Bufsr</ci>
            <ci>CaSR_total</ci>
          </apply>
        </apply>
      </apply>
      <apply><eq/>
        <ci>CaSR_buf</ci>
        <apply><divide/>
          <apply><minus/>
            <apply><root/>
              <apply><minus/>
                <apply><power/>
                  <ci>bCaSR</ci>
                  <cn cellml:units="dimensionless">2.0</cn>
                </apply>
                <apply><times/>
                  <cn cellml:units="dimensionless">4.0</cn>
                  <ci>aCaSR</ci>
                  <ci>cCaSR</ci>
                </apply>
              </apply>
            </apply>
            <ci>bCaSR</ci>
          </apply>
          <apply><times/>
            <cn cellml:units="dimensionless">2.0</cn>
            <ci>aCaSR</ci>
          </apply>
        </apply>
      </apply>
      <apply><eq/>
        <ci>CaSR</ci>
        <apply><minus/>
          <ci>CaSR_total</ci>
          <ci>CaSR_buf</ci>
        </apply>
      </apply>
      <apply id="CaSR_total_diff"><eq />
        <apply><diff/>
          <bvar><ci>time</ci></bvar>
          <ci> CaSR_total </ci>
        </apply>
        <apply><times/>
          <apply><divide/>
            <ci>vC</ci>
            <ci>vSR</ci>
          </apply>
          <apply><minus/>
            <ci>Jup</ci>
            <apply><plus/>
              <ci>Jleak</ci>
              <ci>Jrel</ci>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  
</model>
