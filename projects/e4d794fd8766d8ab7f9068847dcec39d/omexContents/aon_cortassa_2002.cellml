                  <apply>
                     <times/>
                     <ci>kf</ci>
                     <ci>CD</ci>
                  </apply>
                  <apply>
                     <times/>
                     <ci>kb</ci>
                     <ci>CT</ci>
                     <ci>GDP</ci>
