   <component name="V_PFK">
      <variable units="flux" public_interface="out" name="V_PFK"/>
      <variable units="millimolar" public_interface="in" name="G6P"/>
      <variable units="millimolar" public_interface="in" name="ATP"/>
      <variable units="millimolar" public_interface="in" name="AMP"/>
      <variable units="millimolar" name="KG6P_r" initial_value="1"/>
      <variable units="millimolar" name="KATP_r" initial_value="0.06"/>
      <variable units="millimolar" name="KAMP_r" initial_value="0.025"/>
      <variable units="dimensionless" name="cATP" initial_value="1"/>
      <variable units="dimensionless" name="cAMP" initial_value="0.019"/>
      <variable units="dimensionless" name="cG6P" initial_value="0.0005"/>
      <variable units="dimensionless" name="Lo" initial_value="25000"/>
      <variable units="dimensionless" name="gr" initial_value="10"/>
      <variable units="dimensionless" name="n1" initial_value="2"/>
      <variable units="flux" name="V_PFK_max" initial_value="30"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>V_PFK</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
                        <apply>
                           <divide/>
                           <apply>
                              <times/>
                              <ci>V_PFK_max</ci>
                              <ci>gr</ci>
                              <ci>G6P</ci>
                           </apply>
                           <ci>KG6P_r</ci>
                        </apply>
                        <ci>ATP</ci>
                     </apply>
                     <ci>KATP_r</ci>
                  </apply>
                  <apply>
                     <power/>
                     <apply>
                        <plus/>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                           <divide/>
                           <ci>G6P</ci>
                           <ci>KG6P_r</ci>
                        </apply>
                        <apply>
                           <divide/>
                           <ci>ATP</ci>
                           <ci>KATP_r</ci>
                        </apply>
                        <apply>
                           <divide/>
                           <apply>
                              <times/>
                              <apply>
                                 <divide/>
                                 <apply>
                                    <times/>
                                    <ci>gr</ci>
                                    <ci>G6P</ci>
                                 </apply>
                                 <ci>KG6P_r</ci>
                              </apply>
                              <ci>ATP</ci>
                           </apply>
                           <ci>KATP_r</ci>
                        </apply>
