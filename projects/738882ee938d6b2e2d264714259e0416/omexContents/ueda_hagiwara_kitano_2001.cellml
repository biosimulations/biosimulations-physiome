    <variable name="a" public_interface="in" units="dimensionless"/>
    <variable name="r" public_interface="in" units="dimensionless"/>
    
    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/> 
        <apply>
          <diff/> 
          <bvar>
            <ci>time</ci>
          </bvar> 
          <ci>Per_m</ci> 
        </apply>
        <apply>
          <minus/>
          <apply>
            <plus/>
            <ci> C1 </ci>
            <apply>
              <times/>
              <ci> S1 </ci>
              <apply>
                <divide/>
                <apply>
                  <plus/>
                  <apply>
                    <power/>
                    <apply>
                      <divide/>
                      <ci> CC_n </ci>
                      <ci> A1 </ci>
                    </apply>
                    <ci> a </ci>
                  </apply>
                  <ci> B1 </ci>
                </apply>
                <apply>
                  <plus/>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless"> 1.0 </cn>
                  <apply>
                    <power/>
                    <apply>
                      <divide/>
                      <ci> PT_n </ci>
                      <ci> R1 </ci>
                    </apply>
                    <ci> r </ci>
                  </apply>
                  <apply>
                    <power/>
                    <apply>
                      <divide/>
                      <ci> CC_n </ci>
                      <ci> A1 </ci>
                    </apply>
                    <ci> a </ci>
                  </apply>
                  <ci> B1 </ci>
                </apply>
              </apply>
            </apply>
          </apply>
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> D1 </ci>
              <apply>
                <divide/>
                <ci> Per_m </ci>
                <apply>
                  <plus/>
                  <ci> L1 </ci>
                  <ci> Per_m </ci>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> D0 </ci>
              <ci> Per_m </ci>
            </apply>
          </apply>
        </apply>
      </apply>    
    </math>
