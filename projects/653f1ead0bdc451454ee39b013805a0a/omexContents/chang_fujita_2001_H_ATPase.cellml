    <math xmlns="http://www.w3.org/1998/Math/MathML"> 
      <apply>
        <eq/>
        <ci> Ha </ci> 
        <apply>
          <times/>
          <ci> H </ci>
          <apply>
            <exp/>
            <apply>
              <divide/>
              <apply>
                <times/> 
                <ci> F </ci>
                <apply>
                  <plus/>
                  <ci> psi </ci>
                  <ci> psi_ </ci>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> R </ci>
                <ci> T </ci>
              </apply>
            </apply>
          </apply>
        </apply>  
      </apply> 
    </math> 
