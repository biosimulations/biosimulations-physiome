      <apply id="k13_calculation">
        <eq/>
        <ci> k13 </ci>
        <apply>
          <times/>
          <cn type="e-notation" cellml:units="first_order_rate_constant">2.41<sep/>0</cn>
          <ci> W </ci>
        </apply>
      </apply>
        
      <apply id="k14_calculation">
        <eq/>
        <ci> k14 </ci>
        <apply>
          <times/>
          <cn type="e-notation" cellml:units="first_order_rate_constant">2.41<sep/>0</cn>
          <ci> W </ci>
        </apply>
      </apply>

      <apply id="k15_calculation">
        <eq/>
        <ci> k15 </ci>
        <apply>
          <times/>
          <cn type="e-notation" cellml:units="first_order_rate_constant">4.8<sep/>-1</cn>
          <ci> W </ci>
        </apply>
      </apply>
        
      <apply id="k16_calculation">
        <eq/>
        <ci> k16 </ci>
        <apply>
          <times/>
          <cn type="e-notation" cellml:units="first_order_rate_constant">4.8<sep/>-1</cn>
          <ci> W </ci>
        </apply>
      </apply>

      <apply id="k17_calculation">
        <eq/>
        <ci> k17 </ci>
        <apply>
          <times/>
          <cn type="e-notation" cellml:units="first_order_rate_constant">2.41<sep/>0</cn>
