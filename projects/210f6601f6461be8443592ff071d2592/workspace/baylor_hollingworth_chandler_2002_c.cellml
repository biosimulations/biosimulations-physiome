   <units name="micromolar">
      <unit prefix="micro" units="mole"/>
      <unit exponent="-1" units="litre"/>
   </units>
   
   <units name="first_order_rate_constant">
      <unit exponent="-1" units="second"/>
   </units>
  
   <units name="second_order_rate_constant">
      <unit exponent="-1" units="micromolar"/>
      <unit exponent="-1" units="second"/>
   </units>
  

   <component name="environment">
      <variable name="time" public_interface="out" units="second"/>
   </component>
   
   
   <component cmeta:id="Ca" name="Ca">
      <variable cmeta:id="id_00001" initial_value="0.05" name="Ca" public_interface="out" units="micromolar"/>
      
      <variable name="k1" public_interface="in" units="second_order_rate_constant"/>
      <variable name="k1_" public_interface="in" units="first_order_rate_constant"/>
      <variable name="Parv" public_interface="in" units="micromolar"/>
      <variable name="CaParv" public_interface="in" units="micromolar"/>
      <variable name="time" public_interface="in" units="second"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
        <eq/>
          <apply>
          <diff/> 
            <bvar>
            <ci>time</ci>
          </bvar> 
            <ci> Ca </ci> 
          </apply>
          <apply>
          <minus/>
            <apply>
            <times/>
              <ci> k1_ </ci>
              <ci> CaParv </ci>
            </apply>
            <apply>
            <times/>
              <ci> k1 </ci>
              <ci> Ca </ci>
              <ci> Parv </ci>
            </apply>
          </apply>
        </apply>
      </math>
   </component>
   
   <component cmeta:id="Parv" name="Parv">
      <variable initial_value="1500.0" name="Parv" public_interface="out" units="micromolar"/>
     
      <variable name="k1" public_interface="in" units="second_order_rate_constant"/>
      <variable name="k1_" public_interface="in" units="first_order_rate_constant"/>
      <variable name="k2" public_interface="in" units="second_order_rate_constant"/>
      <variable name="k2_" public_interface="in" units="first_order_rate_constant"/>
      <variable name="Ca" public_interface="in" units="micromolar"/>
      <variable name="CaParv" public_interface="in" units="micromolar"/>
      <variable name="Mg" public_interface="in" units="micromolar"/>
      <variable name="MgParv" public_interface="in" units="micromolar"/>
      <variable name="time" public_interface="in" units="second"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
        <eq/>
          <apply>
          <diff/> 
            <bvar>
            <ci>time</ci>
          </bvar> 
            <ci> Parv </ci> 
          </apply>
          <apply>
          <minus/>
            <apply>
            <plus/>
              <apply>
              <times/>
                <ci> k1_ </ci>
                <ci> CaParv </ci>
              </apply>
              <apply>
              <times/>
                <ci> k2_ </ci>
                <ci> MgParv </ci>
              </apply>
            </apply>
            <apply>
            <plus/>
              <apply>
              <times/>
                <ci> k1 </ci>
                <ci> Ca </ci>
                <ci> Parv </ci>
              </apply>
              <apply>
              <times/>
                <ci> k2 </ci>
                <ci> Mg </ci>
                <ci> Parv </ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </math>
   </component>
  
   <component cmeta:id="CaParv" name="CaParv">
      <variable initial_value="0.01" name="CaParv" public_interface="out" units="micromolar"/>
      
      <variable name="k1" public_interface="in" units="second_order_rate_constant"/>
      <variable name="k1_" public_interface="in" units="first_order_rate_constant"/>
      <variable name="Parv" public_interface="in" units="micromolar"/>
      <variable name="Ca" public_interface="in" units="micromolar"/>
      <variable name="time" public_interface="in" units="second"/>
      
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
        <eq/>
          <apply>
          <diff/> 
            <bvar>
            <ci>time</ci>
          </bvar> 
            <ci> CaParv </ci> 
          </apply>
          <apply>
          <minus/>
            <apply>
            <times/>
              <ci> k1 </ci>
              <ci> Ca </ci>
              <ci> Parv </ci>
            </apply>
            <apply>
            <times/>
              <ci> k1_ </ci>
              <ci> CaParv </ci>
            </apply>
          </apply>
        </apply>
      </math>
   </component>
   
   <component cmeta:id="Mg" name="Mg">
      <variable cmeta:id="id_00002" initial_value="1000.0" name="Mg" public_interface="out" units="micromolar"/>
     
      <variable name="k2" public_interface="in" units="second_order_rate_constant"/>
      <variable name="k2_" public_interface="in" units="first_order_rate_constant"/>
