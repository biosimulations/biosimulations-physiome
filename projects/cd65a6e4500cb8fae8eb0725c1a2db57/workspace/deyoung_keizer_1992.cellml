        <math id="6" xmlns="http://www.w3.org/1998/Math/MathML">
            <apply id="P_open_calculation">
                <eq/>
                <ci> P_open </ci>
                <apply>
                    <power/>
                    <apply>
                        <divide/>
                        <apply>
                            <times/>
                            <ci> Ca_i </ci>
                            <ci> IP3 </ci>
                            <ci> d2 </ci>
                        </apply>
                        <apply>
                            <times/>
                            <apply>
                                <plus/>
                                <apply>
                                    <times/>
                                    <ci> Ca_i </ci>
                                    <ci> IP3 </ci>
                                </apply>
                                <apply>
                                    <times/>
                                    <ci> IP3 </ci>
                                    <ci> d2 </ci>
                                </apply>
                                <apply>
                                    <times/>
                                    <ci> d1 </ci>
                                    <ci> d2 </ci>
                                </apply>
                                <apply>
                                    <times/>
                                    <ci> Ca_i </ci>
                                    <ci> d3 </ci>
                                </apply>
                            </apply>
                            <apply>
                                <plus/>
                                <ci> Ca_i </ci>
                                <ci> d5 </ci>
                            </apply>
                        </apply>
                    </apply>
                    <cn cellml:units="dimensionless"> 3.0 </cn>
                </apply>
            </apply>
        </math>
