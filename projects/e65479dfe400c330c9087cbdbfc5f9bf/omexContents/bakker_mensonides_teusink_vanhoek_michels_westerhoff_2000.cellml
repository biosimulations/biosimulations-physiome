              </apply>
              <apply>
                <divide/>
                <ci> Gly </ci>
                <ci> K_Gly </ci>
              </apply>
            </apply>
            <apply>
              <plus/>
              <cn cellml:units="dimensionless">1</cn>
              <apply>
                <divide/>
                <ci> ADP </ci>
                <ci> K_ADP </ci>
              </apply>
              <apply>
                <divide/>
                <ci> ATP </ci>
                <ci> K_ATP </ci>
              </apply>
            </apply>
          </apply>
        </apply>
