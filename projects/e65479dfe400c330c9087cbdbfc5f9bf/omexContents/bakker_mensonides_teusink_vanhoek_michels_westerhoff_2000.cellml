          <divide/>
		  <apply>
            <divide/>
			<apply>
              <times/>
              <ci> vPK_max </ci>
			  <apply>
                <power/>
                <apply>
                  <divide/>
				  <ci> PEP </ci>
				  <apply>
                    <times/>
                    <cn cellml:units="millimolar">0.34</cn>
                    <apply>
                      <plus/>
                      <cn cellml:units="dimensionless">1</cn>
                      <apply>
                        <divide/>
                        <ci> ATP </ci>
                        <cn cellml:units="millimolar">0.57</cn>
                      </apply>
                      <apply>
                        <divide/>
                        <ci> ADP </ci>
                        <cn cellml:units="millimolar">0.64</cn>
                      </apply>
                    </apply>
                  </apply>
                </apply>
				<ci> n </ci>
              </apply>
			  <ci> ADP </ci>
			</apply>
			<ci> KM_ADP </ci>
	      </apply>
		  <apply>
            <times/>
			<apply>
              <plus/>
              <cn cellml:units="dimensionless">1</cn>
              <apply>
                <power/>
                <apply>
                  <divide/>
                  <ci> PEP </ci>
				  <apply>
                    <times/>
                    <cn cellml:units="millimolar">0.34</cn>
                    <apply>
                      <plus/>
                      <cn cellml:units="dimensionless">1</cn>
                      <apply>
                        <divide/>
                        <ci> ATP </ci>
                        <cn cellml:units="millimolar">0.57</cn>
                      </apply>
                      <apply>
                        <divide/>
                        <ci> ADP </ci>
                        <cn cellml:units="millimolar">0.64</cn>
                      </apply>
                    </apply>
                  </apply>
                </apply>
				<ci> n </ci>
			  </apply>
			</apply>
			<apply>
              <plus/>
              <cn cellml:units="dimensionless">1</cn>
              <apply>
                <divide/>
                <ci> ADP </ci>
                <ci> KM_ADP </ci>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
