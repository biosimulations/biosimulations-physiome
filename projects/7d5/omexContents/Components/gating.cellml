<?xml version='1.0' encoding='UTF-8'?>
<model name="gating_eq" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="unit.cellml">
        <units name="per_ms" units_ref="per_ms"/>
        <units name="mV" units_ref="mV"/>
        <!--        unit mM using unit mM;-->
        <units name="mV_ms" units_ref="mV_ms"/>
        <units name="ms" units_ref="ms"/>
        <!--        unit pA_per_pF using unit pA_per_pF;
        unit nS_per_pF using unit nS_per_pF;-->
    </import>
    <component name="gating">
        <variable initial_value="-75.6" name="v" public_interface="out" units="mV"/>
        <variable name="alpha" public_interface="out" units="per_ms"/>
        <variable name="beta" public_interface="out" units="per_ms"/>
        <variable name="tau" public_interface="out" units="ms"/>
        <variable name="act_inact" public_interface="out" units="dimensionless"/>
        <variable name="x1" public_interface="in" units="dimensionless"/>
        <variable name="x2" public_interface="in" units="dimensionless"/>
        <variable name="x5" public_interface="in" units="dimensionless"/>
        <variable name="x6" public_interface="in" units="dimensionless"/>
        <variable name="x7" public_interface="in" units="dimensionless"/>
        <variable name="x4" public_interface="out" units="dimensionless"/>
        <variable name="x3" public_interface="out" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>x4</ci>
                <apply>
                    <divide/>
                    <cn cellml:units="dimensionless">1</cn>
                    <apply>
                        <plus/>
                        <apply>
                            <divide/>
                            <cn cellml:units="dimensionless">1</cn>
                            <ci>x2</ci>
                        </apply>
                        <apply>
                            <divide/>
                            <cn cellml:units="dimensionless">1</cn>
                            <ci>x6</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>x3</ci>
                <apply>
                    <times/>
                    <ci>x5</ci>
                    <ci>x1</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>alpha</ci>
                <apply>
                    <times/>
                    <ci>x1</ci>
                    <apply>
                        <exp/>
                        <apply>
                            <divide/>
                            <ci>v</ci>
                            <ci>x2</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>beta</ci>
                <apply>
                    <times/>
                    <ci>x3</ci>
                    <apply>
                        <exp/>
                        <apply>
                            <divide/>
                            <ci>v</ci>
                            <ci>x4</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>tau</ci>
                <apply>
                    <plus/>
                    <apply>
                        <divide/>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                            <plus/>
                            <ci>alpha</ci>
                            <ci>beta</ci>
                        </apply>
                    </apply>
                    <ci>x7</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>act_inact</ci>
                <apply>
                    <divide/>
                    <ci>alpha</ci>
                    <apply>
                        <plus/>
                        <ci>alpha</ci>
                        <ci>beta</ci>
                    </apply>
                </apply>
            </apply>
        </math>
        <!--        //Equation for Ik1 gatting-->
        <!--        var alpha_new: per_ms {pub: out};
        var beta_new: per_ms {pub: out};-->
        <!--        //        var tau: ms {pub: out};-->
        <!--        var act_inact_new: dimensionless {pub: out};-->
        <!--        //        var var_1: nS_per_pF {pub: in};-->
        <!--        var var_2: per_ms {pub: in};
        var var_3: mV {pub: in};
        var var_4: mV {pub: in};
        var var_5: mV_ms {pub: in};
        var var_6: mV {pub: in};-->
        <!--        //        //        var x3: dimensionless {pub: out};-->
        <!--        alpha_new = var_2*exp((v+var_4)/var_3);
        beta_new = exp((v+var_6)/var_5);-->
        <!--        //        //        tau = 1{dimensionless}/(alpha+beta)+x7;-->
        <!--        act_inact_new = alpha_new/(alpha_new+beta_new);-->
    </component>
</model>
