<?xml version='1.0' encoding='UTF-8'?>
<model name="Current_eq" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="unit.cellml">
        <units name="mV" units_ref="mV"/>
        <units name="mM" units_ref="mM"/>
        <units name="pA_per_pF" units_ref="pA_per_pF"/>
        <units name="nS_per_pF" units_ref="nS_per_pF"/>
    </import>
    <component name="Current_pCa">
        <variable name="ca_i" public_interface="in" units="mM"/>
        <variable name="i_PCa" public_interface="out" units="pA_per_pF"/>
        <variable initial_value="0.2625" name="g_PCa" public_interface="out" units="pA_per_pF"/>
        <variable initial_value="0.0005" name="KPCa" public_interface="out" units="mM"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>i_PCa</ci>
                <apply>
                    <divide/>
                    <apply>
                        <times/>
                        <ci>g_PCa</ci>
                        <ci>ca_i</ci>
                    </apply>
                    <apply>
                        <plus/>
                        <ci>ca_i</ci>
                        <ci>KPCa</ci>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
</model>
